}

enum    _:E_BANK_LOGTYPE
{
    TYPE_NONE,
    TYPE_LOGIN,
    TYPE_DEPOSIT,
    TYPE_WITHDRAW,
    TYPE_TRANSFER,
    TYPE_PASSCHANGE
}

enum    E_ATM
{
    // saved
    Float: atmX,
    Float: atmY,
    Float: atmZ,
