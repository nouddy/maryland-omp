task VremeDatum[1000]()
{
	new dan, godina, mesec;
	getdate(godina, mesec, dan);
	new sati,minuti;
	gettime(sati, minuti);
	new stringic[40]; new stringicc[40];
    format(stringic, sizeof(stringic), "%s%d:%s%d", (sati < 10) ? ("0") : (""), sati, (minuti < 10) ? ("0") : (""), minuti);
    format(stringicc, sizeof(stringicc), "%d/%s%d/%s%d", dan, ((mesec < 10) ? ("0") : ("")), mesec, (godina < 10) ? ("0") : (""), godina);
    TextDrawSetString(Global_TD[78], stringic);
    TextDrawSetString(Global_TD[79], stringicc);
	return Y_HOOKS_CONTINUE_RETURN_1;
}

