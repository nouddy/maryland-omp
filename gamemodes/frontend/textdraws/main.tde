#include <ysilib\YSI_Coding\y_hooks>
//////////////////////////////////////// Textdraws Imena (Defines) ///////////////////////////////////////////////////

//new PlayerText:BankaIgraca[MAX_PLAYERS];
/////////////////////////////////////// Public za sve PlayerTdove  ////////////////////////////////////////////////////


hook OnGameModeInit()
{
    print("frontend/main.tde loaded");

    CreateTextdraws();		//Stock za tdove

    return Y_HOOKS_CONTINUE_RETURN_1;
}

hook OnGameModeExit() {


    for(new i=0; i < sizeof Global_TD; i++)
    {
        if(Global_TD[i] == INVALID_TEXT_DRAW) continue;

        TextDrawDestroy(Global_TD[i]);
        Global_TD[i] = INVALID_TEXT_DRAW;
    }

    return Y_HOOKS_CONTINUE_RETURN_1;
}

hook OnPlayerConnect(playerid)
{

    for(new i=0; i < sizeof Player_TDs[]; i++)
    {
        Player_TDs[playerid][i] = INVALID_PLAYER_TEXT_DRAW;
    }

    return Y_HOOKS_CONTINUE_RETURN_1;
}

hook OnPlayerDisconnect(playerid, reason) {

    for(new i=0; i < sizeof Player_TDs[]; i++)
    {
        PlayerTextDrawDestroy(playerid, PlayerText:Player_TDs[playerid][i]);
        Player_TDs[playerid][i] = INVALID_PLAYER_TEXT_DRAW;
    }

    return Y_HOOKS_CONTINUE_RETURN_1;
}

/////////////////////////////////////////////////////////////////////////////////////////////////////////


stock ToggleGlobalTextDraw(const playerid, const bool:option) {

    if(option) {

        for(new i=0; i < sizeof Global_TD; i++)
        {
            if(Global_TD[i] == INVALID_TEXT_DRAW) continue;

            TextDrawShowForPlayer(playerid, Global_TD[i]);
        }
    }
    else {

        for(new i=0; i < sizeof Global_TD; i++)
        {
            if(Global_TD[i] == INVALID_TEXT_DRAW) continue;

            TextDrawHideForPlayer(playerid, Global_TD[i]);
        }
    }
    return 1;
}

stock TogglePlayerTextDraw(playerid, const bool:option) {

    if(option) {

        CreatePlayerTextDraws(playerid);

        for(new i=0; i < sizeof Player_TDs[]; i++)
        {
            if(Player_TDs[playerid][i] == INVALID_PLAYER_TEXT_DRAW) continue;

            PlayerTextDrawShow(playerid, Player_TDs[playerid][i]);
            
            printf(" PlayerTD: (%d)", i);
        }
        
        // PlayerTextDraw_UpdateModel(playerid, Player_TDs[playerid][1], GetPlayerSkin(playerid));
	    // PlayerTextDrawSetString(playerid, Player_TDs[playerid][0], "%s", ReturnCharacterName(playerid));
    }
    else {

        for(new i=0; i < sizeof Player_TDs[]; i++)
        {
            if(Player_TDs[playerid][i] == INVALID_PLAYER_TEXT_DRAW) continue;

            PlayerTextDrawHide(playerid, Player_TDs[playerid][i]);
            PlayerTextDrawDestroy(playerid, Player_TDs[playerid][i]);
            Player_TDs[playerid][i] = INVALID_PLAYER_TEXT_DRAW;
        
        }
    }
    return 1;

}

