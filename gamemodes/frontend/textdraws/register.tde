stock IsPlayerRegisterTDMail(playerid, PlayerText:textdrawid)
{
    if(textdrawid == INVALID_PLAYER_TEXT_DRAW) return false;
    if(pRegisterTD[playerid][TD_TYPE_REG_MAIL] == textdrawid) return true;
    return false;
}

stock IsPlayerRegisterTDPassword(playerid, PlayerText:textdrawid)
{
    if(textdrawid == INVALID_PLAYER_TEXT_DRAW) return false;
    if(pRegisterTD[playerid][TD_TYPE_REG_PASS] == textdrawid) return true;
    return false;
}

stock IsPlayerRegisterTDConfirm(playerid, PlayerText:textdrawid)
{
    if(textdrawid == INVALID_PLAYER_TEXT_DRAW) return false;
    if(pRegisterTD[playerid][TD_TYPE_REG_CONFIRM] == textdrawid) return true;
    return false;
}

