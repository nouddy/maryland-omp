stock DestroyPlayerRegisterTextDraw(playerid)
{
    for(new i = 0; i < sizeof(pRegisterTD[]); i++)
    {
        if(pRegisterTD[playerid][i] == INVALID_PLAYER_TEXT_DRAW) continue;

        PlayerTextDrawDestroy(playerid, pRegisterTD[playerid][i]);
        pRegisterTD[playerid][i] = INVALID_PLAYER_TEXT_DRAW;
    }
}

stock ShowPlayerRegisterTextDraw(playerid)
{
    for(new i = 0; i < sizeof(pRegisterTD[]); i++)
    {
        if(pRegisterTD[playerid][i] == INVALID_PLAYER_TEXT_DRAW) continue;
        PlayerTextDrawShow(playerid, pRegisterTD[playerid][i]);
    }
}

stock HidePlayerRegisterTextDraw(playerid)
{
    for(new i = 0; i < sizeof(pRegisterTD[]); i++)
    {
        if(pRegisterTD[playerid][i] == INVALID_PLAYER_TEXT_DRAW) continue;
        PlayerTextDrawHide(playerid, pRegisterTD[playerid][i]);
    }
}

