hook OnPlayerClickPlayerTD(playerid, PlayerText:playertextid)
{
    if(playertextid == secondRegister_TD[playerid][55])
	{
		if(RegisterPass[playerid])
			return SendClientMessage(playerid, x_ltorange, "> Vec ste popunili ovo polje.");

		Dialog_Show(playerid, "dialog_regpassword", DIALOG_STYLE_INPUT,
					"Registracija",
					"%s, unesite Vasu zeljenu lozinku: ",
					"Potvrdi", "Izlaz", ReturnPlayerName(playerid)
				);		
	}
	else if(playertextid == secondRegister_TD[playerid][54]) // email
	{
		if(RegisterEmail[playerid])
			return SendClientMessage(playerid, x_ltorange, "> Vec ste popunili ovo polje.");


		Dialog_Show(playerid, "dialog_reggmail", DIALOG_STYLE_INPUT,
			"Email",
			"Upisite vas email, sa kojim cete u slucaju gubitka akaunta vratiti isti.",
			"Unesi", "Izlaz"
			);

	}
	else if(playertextid == Register_PTD[playerid][61]) // dialog_regStateState
	{
		if(RegisterState[playerid])
			return SendClientMessage(playerid, x_ltorange, "> Vec ste popunili ovo polje.");


		Dialog_Show(playerid, "dialog_regState", DIALOG_STYLE_LIST,
			"Izaberite drzavu",
			"Maryland\nLittle Italy\nEgypt",
			"Unesi", "Izlaz"
			);
	}	
	else if(playertextid == Register_PTD[playerid][60])
	{
		if(RegisterAge[playerid])
			return SendClientMessage(playerid, x_ltorange, "> Vec ste popunili ovo polje.");


		Dialog_Show(playerid, "dialog_regages", DIALOG_STYLE_INPUT,
			"Godine",
			"Koliko imate godina: ",
			"Unesi", "Izlaz"
			);

	}
	else if(playertextid == Register_PTD[playerid][62]) // Sex
	{
		if(RegisterSex[playerid])
			return SendClientMessage(playerid, x_ltorange, "> Vec ste popunili ovo polje.");


		Dialog_Show(playerid, "dialog_regpol", DIALOG_STYLE_LIST,
			"Odaberite koji ste Sex",
			"Musko\nZensko",
			"Unesi", "Izlaz"
			);
	}

    else if(playertextid == secondRegister_TD[playerid][56]) {

        if(RegisterEmail[playerid] && RegisterPass[playerid]) {

            //Spawn_PlayerFromRegister(playerid);
        }

        else { SendClientMessage(playerid, x_server, "maryland \187; "c_white"Niste popunili sva polja!"); }

    }

    // 55 54 56

    return Y_HOOKS_CONTINUE_RETURN_1;
