hook OnGameModeInit()
{
    transactions = 
    transactionsBTC = 
    transactionsDOT = 
    transactionsLTC =
    transactionsXRP = 
    transactionsUSDT = 0;

    // Crypto pocetne cene
    CryptoPrices[BitcoinPrice] = 27000;
    CryptoPrices[RipplePrice] = 1;
    CryptoPrices[TetherUSPrice] = 1;
    CryptoPrices[LitecoinPrice] = 93;
    CryptoPrices[PolkadotPrice] = 5;

    if(finishedyet)
    {
        print("backend/crypto_ogy.script loaded");
    }

    return 1;
}

hook OnPlayerLoaded(playerid)
{
    new q[150];
    mysql_format(SQL, q, sizeof(q), "SELECT * FROM player_crypto WHERE crypto_id = '%d'", PlayerInfo[playerid][SQLID]);
    mysql_tquery(SQL, q, "LoadPlayerCrypto", "i", playerid);
    return true;
}

hook OnPlayerConnect(playerid)
{
    ResetPlayersCrypto(playerid);

    return (true);
}

