YCMD:crypto(const playerid, params[], help)
{
    if(help)
    {
        SendClientMessage(playerid, CRYPTO_COLOR, "[Crypto]: Ova komanda vam dozvoljava da kupujete/prodajete crypto valute.");
        return 1;
    }
    if(GledaCryptoTextdraw[playerid])
    {
        BuildCryptoTextDraws(playerid, false);
    }
    else if(!GledaCryptoTextdraw[playerid])
    {
        BuildCryptoTextDraws(playerid, true);
    }
    return Y_HOOKS_CONTINUE_RETURN_1;
}
