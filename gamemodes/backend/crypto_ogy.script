forward LoadPlayerCrypto(playerid);
public LoadPlayerCrypto(playerid)
{
    new rows;
    cache_get_row_count(rows);
    if(!rows)
    {  
        new q[300];
        mysql_format(SQL, q, sizeof(q), 
           "INSERT INTO `player_crypto` (crypto_id, KolicinaBTC, KolicinaETH, KolicinaLTC, KolicinaDOT, KolicinaUSDT) \ 
            VALUES('%d', '0', '0', '0', '0', '0')", PlayerInfo[playerid][SQLID]);
        mysql_tquery(SQL, q);
    }
    else
    {
        cache_get_value_name_float(0, "KolicinaBTC", PlayerCrypto[playerid][countBTC]);
        cache_get_value_name_float(0, "KolicinaETH", PlayerCrypto[playerid][countXRP]);
        cache_get_value_name_float(0, "KolicinaLTC", PlayerCrypto[playerid][countLTC]);
        cache_get_value_name_float(0, "KolicinaUSDT", PlayerCrypto[playerid][countUSDT]);
        cache_get_value_name_float(0, "KolicinaDOT", PlayerCrypto[playerid][countDOT]);
    }
}
