hook OnPlayerKeyStateChange(playerid, KEY:newkeys, KEY:oldkeys)
{
    if(newkeys & KEY_SECONDARY_ATTACK)
    {
        if(IsPlayerInRangeOfPoint(playerid, 3.0, 1482.6442,-1805.5509,18.7957) || IsPlayerInRangeOfPoint(playerid, 3.0, 1451.9034,-1805.8535,13.5661)) //OpstinaLS
        {
            SetPlayerInterior(playerid, 5);
            SetPlayerVirtualWorld(playerid, 5);
            SetPlayerPos(playerid, -2304.2190,1253.0327,-31.6301);
            SetCameraBehindPlayer(playerid);
        }
        else if(IsPlayerInRangeOfPoint(playerid, 3.0, -2304.2190,1253.0327,-31.6301)) //OpstinaLS Izlaz
        {
            SetPlayerInterior(playerid, 0);
            SetPlayerVirtualWorld(playerid, 0);
            SetPlayerPos(playerid, 1482.6442,-1805.5509,18.7957);
            SetCameraBehindPlayer(playerid);
        }
        else if(IsPlayerInRangeOfPoint(playerid, 3.0, 1424.9067,1587.1135,19.1489) && GetPlayerVirtualWorld(playerid) == 6 && GetPlayerInterior(playerid) == 6)
        {
            SetPlayerInterior(playerid, 0);
            SetPlayerVirtualWorld(playerid, 0);
            SetPlayerPos(playerid, 838.5801,-1345.9092,7.1788);
            SetCameraBehindPlayer(playerid);
        }
        else if(IsPlayerInRangeOfPoint(playerid, 3.0, 838.5801,-1345.9092,7.1788) && GetPlayerVirtualWorld(playerid) == 0 && GetPlayerInterior(playerid) == 0)
        {
            SetPlayerInterior(playerid, 6);
            SetPlayerVirtualWorld(playerid, 6);
            SetPlayerPos(playerid, 1424.9067,1587.1135,19.1489);
            SetCameraBehindPlayer(playerid);
        }
        else if(IsPlayerInRangeOfPoint(playerid, 3.0, 1676.1896,-1634.7426,14.2265) && GetPlayerVirtualWorld(playerid) == 0 && GetPlayerInterior(playerid) == 0) // Tehnomedia ulaz
        {
            SetPlayerPos(playerid, 1674.1040, -1634.8239, 14.2263);
            SetCameraBehindPlayer(playerid);
        }
        else if(IsPlayerInRangeOfPoint(playerid, 3.0, 1674.1040, -1634.8239, 14.2263)) // Tehnomedia ulaz
        {
            SetPlayerInterior(playerid, 0);
            SetPlayerVirtualWorld(playerid, 0);
            SetPlayerPos(playerid, 1676.1896,-1634.7426,14.2265);
            SetCameraBehindPlayer(playerid);
        }
        // Hotel Interior
        else if(IsPlayerInRangeOfPoint(playerid, 3.0, 1785.1909,-1296.4972,13.3967) && GetPlayerVirtualWorld(playerid) == 0 && GetPlayerInterior(playerid) == 0) // Hotel ulaz
        {
            SetPlayerVirtualWorld(playerid, 7);
            SetPlayerPos(playerid, 1786.6508,-1300.3116,54.9062);
            SetCameraBehindPlayer(playerid);
        }
        else if(IsPlayerInRangeOfPoint(playerid, 3.0, 1786.6508,-1300.3116,54.9062) && GetPlayerVirtualWorld(playerid) == 7) // Hotel izlaz
        {
            SetPlayerInterior(playerid, 0);
            SetPlayerVirtualWorld(playerid, 0);
            SetPlayerPos(playerid, 1785.1909,-1296.4972,13.3967);
            SetCameraBehindPlayer(playerid);
        }
        // Bank H4RBOR Interior
        else if(IsPlayerInRangeOfPoint(playerid, 3.0, 978.1812, -1188.2797, 23.8752) && GetPlayerVirtualWorld(playerid) == 0 && GetPlayerInterior(playerid) == 0) //Ulaz
        {
            SetPlayerVirtualWorld(playerid, 10);
            SetPlayerPos(playerid, 1201.1162, 2565.0740, -21.6386);
            SetCameraBehindPlayer(playerid);
        }
        else if(IsPlayerInRangeOfPoint(playerid, 3.0, 1201.1162, 2565.0740, -21.6386) && GetPlayerVirtualWorld(playerid) == 10) // izlaz
        {
            SetPlayerInterior(playerid, 0);
            SetPlayerVirtualWorld(playerid, 0);
            SetPlayerPos(playerid, 980.5701, -1190.3965, 23.8752);
            SetCameraBehindPlayer(playerid);
        }
    }
    return Y_HOOKS_CONTINUE_RETURN_1;
}