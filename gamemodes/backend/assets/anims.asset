YCMD:run(playerid, params[], help) {
    if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) { return 1; }
    if(isnull(params)) {
        SendClientMessage(playerid,x_white,"{F9FF52}Koriscenje:{FFFFFF} /run [1-11]");
        return 1;
    }
    
    new animid = strval(params);
    if(animid == 1) { LoopingAnim(playerid,"FAT","FatRun",4.0, true, true, true, true, 1); }
    else if(animid == 2) { LoopingAnim(playerid,"PED","run_player",4.0, true, true, true, true, 1); }
    else if(animid == 3) { LoopingAnim(playerid,"PED","jog_femaleA",4.0, true, true, true, true, 1); }
    else if(animid == 4) { LoopingAnim(playerid,"PED","jog_maleA",4.0, true, true, true, true, 1); }
    else if(animid == 5) { LoopingAnim(playerid,"PED","run_old",4.0, true, true, true, true, 1); }
    else if(animid == 6) { LoopingAnim(playerid,"PED","run_left",4.0, true, true, true, true, 1); }
    else if(animid == 7) { LoopingAnim(playerid,"PED","run_fatold",4.0, true, true, true, true, 1); }
    else if(animid == 8) { LoopingAnim(playerid,"PED","run_gang1",4.0, true, true, true, true, 1); }
    else if(animid == 9) { LoopingAnim(playerid,"PED","run_fat",4.0, true, true, true, true, 1); }
    else if(animid == 10) { LoopingAnim(playerid,"PED","run_right",4.0, true, true, true, true, 1); }
    else if(animid == 11) { LoopingAnim(playerid,"PED","run_wuzi",4.0, true, true, true, true, 1); }
    else { SendClientMessage(playerid,x_white,"{F9FF52}Koriscenje:{FFFFFF} /run [1-11]"); }
    return 1;
}

