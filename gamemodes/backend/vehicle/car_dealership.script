    }

    else if(playertextid == dealership_TD[playerid][46]) {

        // Custom Plates
        if(IsVehicleBicycle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) return SendClientMessage(playerid, x_red, "error \187; "c_white"Ne mozes staviti privatne tablice na biciklo..");
