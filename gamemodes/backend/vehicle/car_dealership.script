    }

    else if(playertextid == dealership_TD[playerid][47]) {

        // Nitrous
        if(IsVehicleBicycle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) return SendClientMessage(playerid, x_red, "error \187; "c_white"Ne mozes ugraditi nitro u biciklo..");
        Dialog_Show(playerid, "dealership_Nitrous", DIALOG_STYLE_MSGBOX, ""c_server"[?] >> "c_white"Nitrous", "Da li zelite ugraditi Nitro Gas u vase novo vozilo\nZa cenu od %d$", "Da", "Ne", DEALERSHIP_NITRO_PRICE);
    }

    else if(playertextid == dealership_TD[playerid][48]) {

        new final_price[MAX_PLAYERS];

        final_price[playerid] = sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Price];

        if(e_CHOSEN_ALARM[playerid]) { final_price[playerid]+= DEALERSHIP_ALARM_PRICE; }
        else if(e_CHOSEN_NITRO[playerid]) { final_price[playerid]+= DEALERSHIP_NITRO_PRICE; }
        else if(e_CHOSEN_XENON[playerid]) { final_price[playerid]+= DEALERSHIP_XENON_PRICE; }
        else if(e_CUSTOM_PLATE[playerid]) { final_price[playerid]+= DEALERSHIP_PLATE_PRICE; }

        // - Dodati kada se odradi player_currency
        // GiveMoneyEx(playerid, -final_price[playerid]);

        new q[ 666 ];

        new id = Iter_Free(iter_Vehicles);

        eVehicle[id][vOwner] = GetCharacterSQLID(playerid);

        eVehicle[id][vModel] = sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model];
        eVehicle[id][vColor][0] = eVehicle[id][vColor][1] = e_CHOSEN_COLOUR[playerid];

        new plate_str[MAX_PLATE_LEN];
        format(plate_str, sizeof plate_str, "%s", ReturnPlayerName(playerid));

        eVehicle[id][vPlate] = plate_str;

        eVehicle[id][vRegDate] = gettime()+2592000; // danasnji datum + 30 dana sto znaci da ce trajati do 19.11.2023 / exmp.

        eVehicle[id][vPos][0] = 950.8036;
        eVehicle[id][vPos][1] = -1759.5557;
        eVehicle[id][vPos][2] = 13.2938;
        eVehicle[id][vPos][3] = 347.8997;

        if(PetroleumVehicle(id)) { eVehicle[id][vFuelType] = FUEL_TYPE_PETROL; }
        else if(DieselVehicle(id)) { eVehicle[id][vFuelType] = FUEL_TYPE_DIESEL; }
        else if(MetanVehicle(id)) { eVehicle[id][vFuelType] = FUEL_TYPE_METAN; }
        else if(ElectricVehicle(id)) { eVehicle[id][vFuelType] = FUEL_TYPE_ELECTRIC; }

        if(IsPlayerInDynamicArea(playerid, italy_Area1) || IsPlayerInDynamicArea(playerid, italy_Area2)) {

            eVehicle[id][vState] = VEHICLE_STATE_ITALY;
        }
        else if(IsPlayerInDynamicArea(playerid, egypt_Area1) || IsPlayerInDynamicArea(playerid, egypt_Area2)) {

            eVehicle[id][vState] = VEHICLE_STATE_EGYPT;
        }
        else eVehicle[id][vState] = VEHICLE_STATE_MARYLAND;

        mysql_format(SQL, q, sizeof q, "INSERT INTO `vehicles` (`vOwner`, `vModel`, `Color1`, `Color2`, `vPlate`, \
								    `vPosX`, `vPosY`, `vPosZ`, `vPosA`, `vRegDate`, `vOil`, `vRange`, `vRangeKM`, `vFuel`, `vFuelType`, `vAlarm`, `vXenon`, \
                                    `vLock`, `vNitro`, `vState`) \
