YCMD:catalogue(playerid, params[], help) 
{
    
    new Float:lb_Pos[3];
    Get3DTextLabelPos(dealership_Catalogue, lb_Pos[0], lb_Pos[1], lb_Pos[2]);

    if(!IsPlayerInRangeOfPoint(playerid, 3.50, lb_Pos[0], lb_Pos[1], lb_Pos[2]))
        return SendClientMessage(playerid, x_server, "maryland \187;"c_white" Ne nalazite se blizu kataloga!");
    
    if(client_PossibleBuy)
        return SendClientMessage(playerid, x_server, "maryland \187;"c_white" Svi agenti su vec zauzeti sacekajte vas red!"); 

    if(IsPlayerInRangeOfPoint(playerid, 3.50, lb_Pos[0], lb_Pos[1], lb_Pos[2]) && dealership_UI[playerid]) {

        Dealership_ShowInterface(playerid, false);

        TogglePlayerControllable(playerid, true);
        SetCameraBehindPlayer(playerid);
        
        DestroyVehicle(temp_Vehicle[playerid]);

    }
    else {

        Dealership_ShowInterface(playerid, true);

        if(!IsValidVehicle(temp_Vehicle[playerid])) {

            InterpolateCameraPos(playerid, 965.256958, -1704.411865, 15.187615, 964.599182, -1695.346191, 15.187615, 3500);
            InterpolateCameraLookAt(playerid, 970.190490, -1703.935180, 14.529376, 969.156066, -1693.405761, 14.502281, 3500);

            temp_Vehicle[playerid] = CreateVehicle(sz_MarylandModels[0][d_Model], 973.3201,-1691.8093,13.6998,135.2371, 3, 3, 1500);
        }
    }

    return 1;
}

YCMD:forcehide(playerid, params[], help) 
{
    Dealership_ShowInterface(playerid, false);
    return 1;
}

hook OnPlayerClickPlayerTD(playerid, PlayerText:playertextid) {

    if(playertextid == dealership_TD[playerid][31]) {

        // Model Up

        e_CHOSEN_MODEL[playerid]++;

        if(e_CHOSEN_MODEL[playerid] > 31 ) {

            e_CHOSEN_MODEL[playerid] = 0;
        }

        PlayerTextDraw_UpdateModel(playerid, dealership_TD[playerid][27], sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model]);

        new tmp_string[64];
        format(tmp_string, sizeof tmp_string, "%s", ReturnVehicleModelName(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model]));
        PlayerTextDrawSetString(playerid, dealership_TD[playerid][30], tmp_string);

        new vprice[64];
        format(vprice, sizeof vprice, "%d$", sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Price]);
        PlayerTextDrawSetString(playerid, dealership_TD[playerid][58], vprice);

        if(PetroleumVehicle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) { PlayerTextDrawSetString(playerid, dealership_TD[playerid][57], "PETROL"); }
        else if(DieselVehicle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) { PlayerTextDrawSetString(playerid, dealership_TD[playerid][57], "DIESEL"); }
        else if(MetanVehicle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) { PlayerTextDrawSetString(playerid, dealership_TD[playerid][57], "METAN"); }
        else if(ElectricVehicle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) { PlayerTextDrawSetString(playerid, dealership_TD[playerid][57], "ELECTRIC"); }
        else if(IsVehicleBicycle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) { PlayerTextDrawSetString(playerid, dealership_TD[playerid][57], "EKO VEHICLE"); }

        PlayerTextDrawShow(playerid, dealership_TD[playerid][57]);

        if(IsValidVehicle(temp_Vehicle[playerid])) {

            DestroyVehicle(temp_Vehicle[playerid]);
            temp_Vehicle[playerid] = CreateVehicle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model], 973.3201,-1691.8093,13.6998,135.2371, e_CHOSEN_COLOUR[playerid], e_CHOSEN_COLOUR[playerid], 1500);
        }
    }

    else if(playertextid == dealership_TD[playerid][32]) {

        // Model Down

        e_CHOSEN_MODEL[playerid]--;

        if(e_CHOSEN_MODEL[playerid] < 0 ) {

            e_CHOSEN_MODEL[playerid] = 31;
        }

        if(IsValidVehicle(temp_Vehicle[playerid])) {

            DestroyVehicle(temp_Vehicle[playerid]);
            temp_Vehicle[playerid] = CreateVehicle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model], 973.3201,-1691.8093,13.6998,135.2371, e_CHOSEN_COLOUR[playerid], e_CHOSEN_COLOUR[playerid], 1500);
        }

        new tmp_string[64];
        format(tmp_string, sizeof tmp_string, "%s", ReturnVehicleModelName(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model]));
        PlayerTextDrawSetString(playerid, dealership_TD[playerid][30], tmp_string);

        new vprice[64];
        format(vprice, sizeof vprice, "%d$", sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Price]);
        PlayerTextDrawSetString(playerid, dealership_TD[playerid][58], vprice);

        if(PetroleumVehicle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) { PlayerTextDrawSetString(playerid, dealership_TD[playerid][57], "PETROL"); }
        else if(DieselVehicle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) { PlayerTextDrawSetString(playerid, dealership_TD[playerid][57], "DIESEL"); }
        else if(MetanVehicle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) { PlayerTextDrawSetString(playerid, dealership_TD[playerid][57], "METAN"); }
        else if(ElectricVehicle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) { PlayerTextDrawSetString(playerid, dealership_TD[playerid][57], "ELECTRIC"); }
        else PlayerTextDrawSetString(playerid, dealership_TD[playerid][57], "EKO VEHICLE");

