    }

    else if(playertextid == dealership_TD[playerid][44]) {

        // Alarm
        if(IsVehicleBicycle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) return SendClientMessage(playerid, x_red, "error \187; "c_white"Ne mozes ugraditi alarm u biciklo..");

        Dialog_Show(playerid, "dealership_Alarm", DIALOG_STYLE_MSGBOX, ""c_server"[?] >> "c_white"Alarm", "Da li zelite ugraditi alarm u vase novo vozilo\nZa cenu od %d$", "Da", "Ne", DEALERSHIP_ALARM_PRICE);
    }

    else if(playertextid == dealership_TD[playerid][45]) {

        // Xenon
        if(IsVehicleBicycle(sz_MarylandModels[e_CHOSEN_MODEL[playerid]][d_Model])) return SendClientMessage(playerid, x_red, "error \187; "c_white"Ne mozes ugraditi xenon u biciklo..");
