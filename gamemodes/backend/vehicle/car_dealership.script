forward Dealership_StreamVehicle(id, playerid);
public Dealership_StreamVehicle(id, playerid) {

    eVehicle[id][vID] = cache_insert_id();
	pvVehicle[id] = CreateVehicle(eVehicle[id][vModel], eVehicle[id][vPos][0], eVehicle[id][vPos][1], eVehicle[id][vPos][2]+4.0, eVehicle[id][vPos][3], eVehicle[id][vColor][0], eVehicle[id][vColor][1], 1500, true);

	new plate_string[MAX_PLATE_LEN];
	format(plate_string, sizeof plate_string, "%s", eVehicle[id][vPlate]);

	SetVehicleNumberPlate(pvVehicle[id], plate_string);

    vehicleBought[playerid] = CreateDynamicCP(eVehicle[id][vPos][0], eVehicle[id][vPos][1], eVehicle[id][vPos][2], 3.45, -1, -1, playerid, 70.0);
    SendClientMessage(playerid, x_red, "Dealership // "c_white"Uspjesno ste kupili vozilo, oznaceno vam je crvenim markerom na lokaciji!");

    TogglePlayerControllable(playerid, true);
    SetCameraBehindPlayer(playerid);
    Dealership_ShowInterface(playerid, false);
    DestroyVehicle(temp_Vehicle[playerid]);

	Iter_Add(iter_Vehicles, id);
}

hook OnPlayerEnterDynamicCP(playerid, STREAMER_TAG_CP:checkpointid)
{

    if(checkpointid == vehicleBought[playerid])
    {
        DestroyDynamicCP(vehicleBought[playerid]);
    }

    return Y_HOOKS_CONTINUE_RETURN_1;
}


