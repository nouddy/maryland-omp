stock SetMoney(id, Money)
{
	ResetPlayerMoney(id);
	PI[id][pMoney] = Money;
	GivePlayerMoney(id, Money);
}
stock g_SetPlayerVirtualWorld(pID, vwid)
{
	PI[pID][pVirWorld] = vwid;
    SetPlayerVirtualWorld(pID, vwid);
	return PI[pID][pVirWorld];
}
stock g_SetPlayerInterior(pID, intid)
{
	PI[pID][pInt] = intid;
    SetPlayerInterior(pID, intid);
	return PI[pID][pInt];
}
stock g_NovacPlus(id, iznos)
{
    KillTimer_H(novactd[id]);
	GivePlayerMoney(id, iznos);
	PI[id][pMoney] += iznos;
	PlusTD(id, iznos);
	novactd[id] = SetTimerEx("PlusSakrij", 5000, false, "d", id);
	SetPVarInt(id, "novactdCalled", 1);
	return 1;
}
stock g_NovacMinus(id, iznos)
{
	GivePlayerMoney(id, -iznos);
	PI[id][pMoney] -= iznos;
	MinusTD(id, iznos);
	novactd[id] = SetTimerEx("MinusSakrij", 5000, false, "d", id);
	SetPVarInt(id, "novactdCalled", 1);
	return 1;
}
stock ResetPlayerMoneyEx(id)
{
	PI[id][pMoney] = 0;
	ResetPlayerMoney(id);
	return 1;
}
stock SetPlayerHealth_H(playerid, Float:hp)
{
	if(hp > 99.0) SetPlayerHealth(playerid, 99);
	else SetPlayerHealth(playerid, hp);
	return 1;
}

stock g_SetPlayerArmour(playerid,Float:armour)
{
	PlayerArmour[playerid] = armour;
	SetPlayerArmour(playerid, PlayerArmour[playerid]);
	return true;
}

