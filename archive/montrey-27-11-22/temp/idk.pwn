PostaviCosLabela(i)
{
	if(IsValidDynamicPickup(CosPick[i])) { DestroyDynamicPickup(CosPick[i]); }
	if(IsValidDynamic3DTextLabel(Text3D:CosLab[i])) { DestroyDynamic3DTextLabel(Text3D:CosLab[i]); }
	if(SI[i][KatPozX] == 0 && SI[i][KatPozY] == 0) { }
	else
	{
		new string[300];
		new tipcosia[60];
		if(SI[i][TipCosa]==1) tipcosia="vozila";
		if(SI[i][TipCosa]==2) tipcosia="motora";
		if(SI[i][TipCosa]==3) tipcosia="letelica";
		if(SI[i][TipCosa]==3) tipcosia="plovila";
		format(string,sizeof(string),"{19B0E3}Ime salona: {ffffff}%s\n{19B0E3}Prodajna vrsta: {ffffff}%s\n{19B0E3}Adresa salona: {FFFFFF}%d\n{19B0E3}Za usluge koristite: {FFFFFF}/katalog",SI[i][ImeCosa],tipcosia,i);
		CosLab[i] = CreateDynamic3DTextLabel(string,0x19B0E3FF,SI[i][KatPozX],SI[i][KatPozY],SI[i][KatPozZ],40.0,INVALID_PLAYER_ID,INVALID_VEHICLE_ID,0,-1,-1,-1,20.0);
		CosPick[i] = CreateDynamicPickup(1239,1,SI[i][KatPozX],SI[i][KatPozY],SI[i][KatPozZ],-1,-1,-1,100.0);
	}
}
