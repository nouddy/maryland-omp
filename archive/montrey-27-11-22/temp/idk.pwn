public OnPlayerUpdate(playerid)
{
    if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
	{
		new Float:vec[3];
    	GetPlayerCameraFrontVector(playerid, vec[0], vec[1], vec[2]);
    	new bool:possible_crasher = false;
     	for (new i = 0; !possible_crasher && i < sizeof(vec); i++)
        if (floatabs(vec[i]) > 10.0)
                    possible_crasher = true;

            if (possible_crasher)
                return 0; //do not send fake data, prevents crash
 	}
 	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
 		new vstring3[24];
 		format(vstring3, sizeof(vstring3), "%d", GetVehicleSpeed(GetPlayerVehicleID(playerid)));
		PlayerTextDrawSetString(playerid, BRZINOMETAR[playerid][7], vstring3);
  	}
  	//
  	if(noclipdata[playerid][cameramode] == 1)
	{
		new keys, ud, lr;
		GetPlayerKeys(playerid, keys, ud, lr);
		if(noclipdata[playerid][mode] && (GetTickCount() - noclipdata[playerid][lastmove] > 100))
		{
		    MoveCamera(playerid);
		}
		if(noclipdata[playerid][udold] != ud || noclipdata[playerid][lrold] != lr)
		{
			if((noclipdata[playerid][udold] != 0 || noclipdata[playerid][lrold] != 0) && ud == 0 && lr == 0)
			{
				StopPlayerObject(playerid, noclipdata[playerid][flyobject]);
				noclipdata[playerid][mode]      = 0;
				noclipdata[playerid][accelmul]  = 0.0;
			}
			else
			{
				noclipdata[playerid][mode] = GetMoveDirectionFromKeys(ud, lr);
				MoveCamera(playerid);
			}
		}
		noclipdata[playerid][udold] = ud; noclipdata[playerid][lrold] = lr;
		return 0;
	}
    // -------------------------------------------------------------------------
    if(GPSAktivan[playerid] == 1)
	{
	    if(IsPlayerInAnyVehicle(playerid))
	    {
		    new Float:VPoz[3], Float:Rotacija, TDString[128];
	 		GetVehiclePos(GetPlayerVehicleID(playerid),VPoz[0],VPoz[1],VPoz[2]);
	 		Rotacija = PointAngle(playerid, VPoz[0],VPoz[1], GPSX[playerid], GPSY[playerid]);
			AttachDynamicObjectToVehicle(GPSObjekat[playerid], GetPlayerVehicleID(playerid), 0.0, 0.0, 1.5, 0.0, 90.0, Rotacija);
			format(TDString, sizeof(TDString), "~y~NAVIGACIJA: ~w~~h~%.1f Metara",GetDistanceBetweenPoints(VPoz[0], VPoz[1], VPoz[2], GPSX[playerid], GPSY[playerid], GPSZ[playerid]));
			GPSTD2(playerid, 1, TDString);
		}
	}
	if(RadioAktiviran[playerid] == 1 && !IsPlayerInAnyVehicle(playerid)) return RadioAktiviran[playerid] = 0, StopAudioStreamForPlayer(playerid);
	if(hPolaganje[playerid] > 0)
	{
		if(IsPlayerInAnyVehicle(playerid))
     	{
     	    if(GetSpeed(playerid) > 120)
     	    {
     	        hKazneniPoeni[playerid]++;
			    TogglePlayerControllable(playerid, 0);
			    SetTimerEx("OdmrzniDebila", 500, false, "d", playerid);
			    SCM(playerid, CRVENA,  "    Instruktor: {FFFFFF}Prekoracili ste brzinu, dobijate jos jedan kazneni poen!");
			}
		}
	}
	if(PoligonK[playerid] >= 1)
	{
		if(IsPlayerInAnyVehicle(playerid))
     	{
     	    if(GetSpeed(playerid) > 120)
     	    {
     	        PoligonK[playerid] = 0;
    			DisablePlayerCheckpoint(playerid);
				RemovePlayerFromVehicle(playerid);
				SCM(playerid, ZUTA, "Instruktor: "SIVA"Pali ste na vozackom ispitu jer ste prekoracili briznu od 120kmh na gradskoj voznji.");
    			SetVehicleToRespawn(GetPlayerVehicleID(playerid));
			}
		}
	}
	// -------------------------- BOOMBOX SISTEM -------------------------------
	if(!BoomboxStream[playerid])
	{
		foreach(Player, i)
	  	{
			if(Boombox[i])
			{
		  		if(IsPlayerInRangeOfPoint(playerid, 30, bpos[i][0], bpos[i][1], bpos[i][2]))
		  		{
				    PlayAudioStreamForPlayer(playerid, BoomboxURL[i], bpos[i][0], bpos[i][1], bpos[i][2], 30, 1);
				    BoomboxPlayer[playerid] = i;
				    BoomboxStream[playerid] = 1;
				}
	  		}
	  	}
	}
	else
	{
		new i = BoomboxPlayer[playerid];
	   	if(!IsPlayerInRangeOfPoint(playerid, 30, bpos[i][0], bpos[i][1], bpos[i][2]))
	   	{
		   	BoomboxStream[playerid] = 0;
		   	BoomboxPlayer[playerid] = -1;
		   	StopAudioStreamForPlayer(playerid);
	   	}
	}
    new Float:health;
  	GetPlayerHealth(playerid,health);
  	if (health < 25.0)
 	{
    	SetPlayerDrunkLevel(playerid,3000);
   	}
   	else
   	{
    	SetPlayerDrunkLevel(playerid,0);
	}
	PoslednjiUpdate[playerid] = GetTickCount();
	return 1;
}
