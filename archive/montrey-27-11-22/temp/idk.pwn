function OnPlayerWeaponCheat(playerid, imeoruzja[], slot)
{
	if(slot < 0 || slot > 12) return 1;
	if(IgracSpawnan[playerid] != 1 || IsPlayerNPC(playerid) || IsPlayerPausedd(playerid)) return OruzjeOtkriveno[slot][playerid] = -1;
	KikovanJe[playerid] = 1;

	ResetPlayerWeaponsEx(playerid);
	OruzjeOtkriveno[slot][playerid] = -1;
	WeaponUpozorenja[playerid]++;
	if(WeaponUpozorenja[playerid] >= 1)
	{
	    for(new i = 0; i < 20; i++) SCM(playerid, CRVENA, " ");
		SCM(playerid, CRVENA, "{F23A0D}[ANTICHEAT]: {FFFFFF}Izbaceni ste sa servera, razlog: Weapon Hack!");
		SetTimerEx("KickTajmer", 100, false, "i", playerid);
	    format(nellys, sizeof(nellys), "{F23A0D}[ANTICHEAT]: {FFFFFF}Igrac %s je kikovan zbog Weapon Hack-a. (oruzje: %s)", Ime_Igraca(playerid), imeoruzja);
	    SendClientMessageToAll(CRVENA, nellys);
	}
	return 1;
}

function OnPlayerAmmoCheat(playerid, imeoruzja[], slot)
{
	if(slot < 0 || slot > 12) return 1;
	if(IgracSpawnan[playerid] != 1 || IsPlayerNPC(playerid) || IsPlayerPausedd(playerid)) return MunicijaOtkriveno[slot][playerid] = -1;
	KikovanJe[playerid] = 1;
	new ime[MAX_PLAYER_NAME];
	GetPlayerName(playerid, ime, sizeof(ime));
	ResetPlayerWeaponsEx(playerid);
	MunicijaOtkriveno[slot][playerid] = -1;
	AmmoUpozorenja[playerid]++;
	if(AmmoUpozorenja[playerid] >= 1)
	{
	    for(new i = 0; i < 20; i++) SCM(playerid, CRVENA, " ");
		SCM(playerid, CRVENA, "{F23A0D}[ANTICHEAT]: {FFFFFF}Izbaceni ste sa servera, razlog: Weapon or Ammo Hack!");
		SetTimerEx("KickTajmer", 100, false, "i", playerid);
	    format(nellys, sizeof(nellys), "{F23A0D}[ANTICHEAT]: {FFFFFF}Igrac %s je kikovan zbog  Weapon/Ammo Hacka-a.", ime);
	    SendClientMessageToAll(CRVENA, nellys);
	}
	return 1;
}

