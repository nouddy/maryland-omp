// ============================================================================> OnplayerDisconnect
public OnPlayerDisconnect(playerid, reason)
{
    if(UlogovanProvera[playerid] == 1)
	{
	    hDobioPay[playerid] = 0;
	    if(Rentano[playerid] != -1) { SetVehicleToRespawn(Rentano[playerid]); Rentano[playerid] = -1; }
	    new str[21], godina, mesec, dan, sat, minut, sekund;
		getdate(godina, mesec, dan);
		gettime(sat, minut, sekund);
		format(str, 21, "%d/%d/%d u %d:%d", dan, mesec, godina, sat, minut);
		format(PI[playerid][PoslednjaAktivnost], 32, str);
		if(hRonilac[playerid] != 0)
		{
		    TextDrawHideForPlayer(playerid, OxygenTD0);
			TextDrawHideForPlayer(playerid, OxygenTD1);
		    DestroyPlayerProgressBar(playerid, oxygen[playerid]);
	    	oxygen[playerid] = INVALID_PLAYER_BAR_ID;
		    hRonilac[playerid] = 0;
		}
		if(gSpecaGa[playerid] != -1)
		{
			SCM(gSpecaGa[playerid], ZUTA, "* Igrac kojeg ste specali je napustio server.");
