		new string[256];
		if(PI[playerid][Clan] > 0)
		{
			format(string,sizeof(string),"[OBAVESTENJE] Vasa organizacija je prosirila svoju vladavinu na jos jednu teritoriju. Clan %s je zauzeo teritoriju koju je kontrolisala organizacija %s",ImeIgraca(playerid), ImeZone(PI[playerid][Clan]));
			FamilyPoruka(PI[playerid][Clan],-1,string);
		}
		else
		{
			format(string,sizeof(string),"{F3FF02}OBAVESTENJE] Vasa organizacija je prosirila svoju vladavinu na jos jednu teritoriju. Clan %s je zauzeo teritoriju koju je kontrolisala organizacija %s",ImeIgraca(playerid), ImeZone(PI[playerid][Clan]));
			FamilyPoruka(PI[playerid][Lider],-1,string);
		}
		new oldteam = GZ[id][gTeam];
		GZ[id][gTeam] = PI[playerid][Clan];
		SacuvajZonu(id);
		DestroyDynamic3DTextLabel(ZonaLabel[id]);
		GangZoneHideForAll(ZonaID[id]);
		GangZoneShowForAll(ZonaID[id],ZonaBoja(GZ[id][gTeam]));
		if(GZ[id][gGlavna] == 0)
		{
			format(string,sizeof(string),"{19B0E3}[ {FFFFFF}TERITORIJA {19B0E3}]\nTeritorija je u vlasnistvu {FFFFFF}%s\n{19B0E3}Ako zelis da je zauzmes kucaj {FFFFFF}/zauzmi\n{19B0E3}Zona: "C_BELA"%d",ImeZone(PI[playerid][Clan]), id);
		}
		else if(GZ[id][gGlavna] == 1)
		{
			format(string,sizeof(string),"{19B0E3}[ {FFFFFF}TERITORIJA {19B0E3}]\nTeritorija je u vlasnistvu {FFFFFF}%s\n{19B0E3}Zona: "C_BELA"%d",ImeZone(PI[playerid][Clan]), id);
		}
		ZonaLabel[id] = CreateDynamic3DTextLabel(string ,0x19B0E3FF,GZ[id][gPickupx], GZ[id][gPickupy], GZ[id][gPickupz],15);
		format(string,sizeof(string),"[OBAVESTENJE] Suparnicka organizacija %s je preuzela jednu od vasih teritorija",ImeZone(PI[playerid][Clan]));
		FamilyPoruka(oldteam,-1,string);
	}
	return 1;
}
function ZonaTimer()
{
	for(new id = 0; id < sizeof(GZ); id++)
	{
		if(GZ[id][gPreuzeto] > 0)
		{
			GZ[id][gPreuzeto] -= 1;
			SacuvajZonu(id);
		}
	}
	return 1;
}

