public LottoIzvlacenje()
{
	new lottostring[256];
	format(lottostring,sizeof(lottostring),"{80C8FE}[DRZAVNA LUTRIJA]: {FFFFFF}Izabran je redni broj: %d",Number[6]);
	SCMTA(GRAD2, lottostring);
	new Winning[MAX_PLAYERS];
	pogodjenih3 = 0;
	pogodjenih4 = 0;
	pogodjenih5 = 0;
	pogodjenih6 = 0;
	pogodjenih7 = 0;
	foreach(Player, i)
	{
		for(new n = 0; n < 7; n++)
		{
        	if(PI[i][pLottoNr] == Number[n])
        	{
            	Winning[i]++;
            	Listic1++;
        	}
         	if(PI[i][pLottoNr2] == Number[n])
			{
            	Winning[i]++;
            	Listic2++;
            }
            if(PI[i][pLottoNr3] == Number[n])
            {
            	Winning[i]++;
            	Listic3++;
            }
            if(PI[i][pLottoNr4] == Number[n])
            {
            	Winning[i]++;
            	Listic4++;
            }
            if(PI[i][pLottoNr5] == Number[n])
            {
            	Winning[i]++;
            	Listic5++;
           }
           if(PI[i][pLottoNr6] == Number[n])
           {
           		Winning[i]++;
           		Listic6++;
           }
           if(PI[i][pLottoNr7] == Number[n])
           {
           		Winning[i]++;
           		Listic7++;
           }
		}
        SendClientMessage(i, GRAD2, " ");
        SendClientMessage(i, GRAD2, "{80C8FE}[DRZAVNA LUTRIJA]: {FFFFFF}Zavrsili smo sa izvlacenjem kuglica!");
        if(PI[i][pLottoNr] > 0 && PI[i][pLottoNr2] > 0 && PI[i][pLottoNr3] > 0 && PI[i][pLottoNr4] > 0 && PI[i][pLottoNr5] > 0 && PI[i][pLottoNr6] > 0 && PI[i][pLottoNr7] > 0)
		{
        	format(lottostring, sizeof(lottostring), "Vasa odigrana kombinacija: %d, %d, %d, %d, %d, %d, %d.", PI[i][pLottoNr],PI[i][pLottoNr2],PI[i][pLottoNr3],PI[i][pLottoNr4],PI[i][pLottoNr5],PI[i][pLottoNr6],PI[i][pLottoNr7]);
            SCM(i, ZUTA, lottostring);
        }
        format(lottostring, sizeof(lottostring), "Dobitna kombinacija: %d, %d, %d, %d, %d, %d, %d", Number[0], Number[1], Number[2], Number[3], Number[4], Number[5], Number[6]);
        SCM(i, ZUTA, lottostring);
        lottostring = "\0";
		Listic1 = 0;
        Listic2 = 0;
        Listic3 = 0;
        Listic4 = 0;
        Listic5 = 0;
        Listic6 = 0;
        Listic7 = 0;
        if(PI[i][pLottoNr] > 0 && PI[i][pLottoNr2] > 0 && PI[i][pLottoNr3] > 0 && PI[i][pLottoNr4] > 0 && PI[i][pLottoNr5] > 0 && PI[i][pLottoNr6] > 0 && PI[i][pLottoNr7] > 0)
		{
        	if(Winning[i] == 0)
            {
            	SCM(i, GRAD2, "{80C8FE}[DRZAVNA LUTRIJA]: {FFFFFF}Zao nam je,danas niste nista dobili,vise srece drugi put.");
            }
            if(Winning[i] == 1)
