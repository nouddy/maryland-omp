YCMD:pretresi(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if(!Policajac(playerid)) return Error(playerid, "Samo pripadnici policije.");
	if(OfficerNaDuznosti[playerid] == 0) return Error(playerid, "Niste uzeli znacku(duznost) pa nemozete koristi tu komandu.");
	if(IsPlayerInAnyVehicle(playerid)) return Error(playerid, "Nemozes koristiti tu komandu kad si u vozilu.");
	new id;
	if(sscanf(params, "u", id)) return Koristi(playerid, "pretresi [ID/Dio Imena]");
	if(!IsPlayerConnected(id)) return Error(playerid, "Igrac je offline.");
	if(!DaljinaIgraca(5.0,playerid,id)) return Error(playerid, "Taj igrac nije blizu tebe.");
	new droga1[24],droga2[24],materijali1[24],oruzije[24],string[128];
	if(PI[id][Kokain] > 0) droga1 = "Poseduje kokain";
	else if(PI[id][Kokain] == 0) droga1 = "Nema kokaina!";
	if(PI[id][Cannabis] > 0) droga2 = "Poseduje Cannabis";
	else if(PI[id][Cannabis] == 0) droga2 = "Nema Cannabis!";
	if(PI[id][pMats] > 0) materijali1 = "Poseduje Materijale";
	else if(PI[id][pMats] == 0) materijali1 = "Nema Materijala!";
	if(GetPlayerWeapon(id) > 0) oruzije = "Poseduje Oruzije";
	else if(GetPlayerWeapon(id) == 0) oruzije = "Nema Oruzije!";
	SCMF(playerid,-1, ""C_ZUTA"|----------------Stvari %s----------------|", Ime_Igraca(id));
	SCMF(playerid,-1, "- Kokain: "C_ZUTA"%s", droga1);
	SCMF(playerid,-1, "- Cannabis: "C_ZUTA"%s", droga2);
	SCMF(playerid,-1, "- Materijali: "C_ZUTA"%s", materijali1);
	SCMF(playerid,-1, "- Oruzije: "C_ZUTA"%s", oruzije);
	format(string, sizeof(string), "* Policajac %s vadi nalog za ptretres.", Ime_Igraca(playerid));
	format(string, sizeof(string), "* Policajac %s pretresa %s.", Ime_Igraca(playerid),Ime_Igraca(id));
	ProxDetector(20.0, playerid, string, PROX,PROX,PROX,PROX,PROX);
	return 1;
}
