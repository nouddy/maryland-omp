	if(SalonInfo[d][KatPozX] == 0 && SalonInfo[d][KatPozY] == 0)return Error(playerid, "Ovaj auto salon nije zavrsen!");
	if(SalonInfo[d][VoPPozX] == 0 && SalonInfo[d][VoPPozY] == 0)return Error(playerid, "Ovaj auto salon nije zavrsen!");
	if(SalonInfo[d][MePPozX] == 0 && SalonInfo[d][MePPozY] == 0)return Error(playerid, "Ovaj auto salon nije zavrsen!");
