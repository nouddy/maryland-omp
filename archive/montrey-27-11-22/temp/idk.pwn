task cAntiCheat[6000]()
{
    foreach(Player, i)
    {
        if(KikovanJe[i] == 0)
        {
	        ZabranjenaOruzja();
	        ProveriIgraca2();
	        GetPlayerHealth(i, HealthProvera);
			GetPlayerArmour(i, ArmourProvera);
			GetPlayerPos(i,animXYZ[0],animXYZ[1],animXYZ[2]);
	        //==================================================================
	       	if(HealthProvera > 99.0 && HealthSynced[i] == 1 && AntiSpawnKill[i] == 0) OnPlayerHealthHack(i, HealthProvera);
	        //==================================================================
	        if(ArmourProvera > PlayerArmour [i] ) { CallLocalFunction("AntiCheat","dd",i,acArmour); }
	        //==================================================================
	        if(GetPlayerSpecialAction(i) == SPECIAL_ACTION_USEJETPACK  && PI[i][Admin] < 1) { CallLocalFunction("AntiCheat","dd",i,acJetPack); }
	        //==================================================================
	        if(GetPlayerState(i) == PLAYER_STATE_DRIVER)
	        {
	          	GetVehicleVelocity(GetPlayerVehicleID(i),X,Y,Z);
	         	if(floatround(X) > 1 || floatround(Y) > 1 || floatround(Z) > 1)
	       		{
	         		CallLocalFunction("AntiCheat","dd",i,acInVehicleSpeed);
	         	}
	        }
	        new anim = GetPlayerAnimationIndex(i);
	        if((anim >= 1538) && (anim <= 1542) && animXYZ[2] > 5 && !IsPlayerInWater(i))
	     	{
                CallLocalFunction("AntiCheat","dd",i,acFly);
			}
	        //==================================================================
	        if((Vel [0] <= -0.800000  || Vel [1] <= -0.800000 || Vel [2] <= -0.800000) && (anim == 1008 || anim == 1539)) { CallLocalFunction("AntiCheat","dd",i,acFly); }
	        //==================================================================
	        if(IsPlayerInAnyVehicle(i) && PI[i][Admin] < 1)
			{
	           if(GetVehicleHealth(GetPlayerVehicleID(i), vHealth) > 1000) { CallLocalFunction("AntiCheat","dd",i,acVhealth); }
			}
			//==================================================================
			if(GetPlayerMoney(i) != PI[i][pMoney]) { CallLocalFunction("AntiCheat","dd",i,acMoney); }
			//==================================================================
			if(KiwiBot[0][kb_PingKicker])
			{
				if(GetPlayerPing(i) > MAX_PING) ImaVisokPing[i]++;
			    else ImaVisokPing[i] = 0;
			    if(ImaVisokPing[i] > 5)
 				{
			        SCMF(i, CRVENA, "MG ANTICHEAT | Izbaceni ste zbog previsokog pinga (%d ms)!", GetPlayerPing(i));
			        format(nellys, sizeof(nellys), "MG ANTICHEAT | %s je izbacen zbog previsokog pinga! (%d ms)", Ime_Igraca(i), GetPlayerPing(i));
			        AdminPoruka(TAMNOCRVENA, nellys);
			        KickPL(i);
				}
			}
   		}
	}
	return 1;
}

