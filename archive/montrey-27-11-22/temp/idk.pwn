	for(new bs = 0; bs < MAX_DROGA; bs++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 3.0, DrogaInfo[bs][drX], DrogaInfo[bs][drY], DrogaInfo[bs][drZ]))
		{
			b = bs;
		 	break;
		}
	}
	if(b == -1) return Error(playerid, "Niste blizu nijedne sadnice.");
	if(IgracPolicajac(playerid)) return Error(playerid, "Ne mozete to, vi ste pripadnik PD/FBI.");
	if(DrogaInfo[b][drTime] > 0) return Error(playerid, "Ova sadnica droge jos nije zrela.");
	if(strcmp(DrogaInfo[b][drPosadio], Ime_Igraca(playerid)) == 0)
	{
		if(DrogaInfo[b][drVrsta] == 2)
		{
			ObrisiDrogu(b);
			new grams = randomExx(1,4);
			PI[playerid][Kokain] += grams;
			SCMF(playerid, SVETLOPLAVA, "** Ubrali ste kokain,dobili ste %d grama.", grams);
