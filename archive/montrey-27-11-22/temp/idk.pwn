YCMD:prisoned(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if (PI[playerid][GM] >= 2 || PI[playerid][Admin] >= 1)
	{
		if(PI[playerid][Admin] < 6) {
	    	if(AdminDuty[playerid] == 0 && HelperDuty[playerid] == 0) return SCM(playerid,-1,""SERVERPLAVA"(MG:RPG) "C_BELA"Morate biti na Admin & Helper duznosti.");
 		}
		new vrsta[15], x = 0, string[256];
		SCM(playerid, SVETLOCRVENA, "_______________ Spisak zatvorenih igraca _______________");
		foreach(Player, i)
		{
			if(PI[i][Zatvor] != 0)
			{
				if(PI[i][Zatvor] == 1) vrsta = "PD Prison";
				else if(PI[i][Zatvor] == 2) vrsta = "Area51";
				else vrsta = "N/A";
				format(string, sizeof(string), " %s[ID:%d] | Zatvor: %s | Vreme: %s", Ime_Igraca(i), i, vrsta, KonvertujVreme(PI[i][pZatvorenVreme]));
				SCM(playerid, BELA, string);
				x++;
			}
		}
		if(x == 0) return Error(playerid, "Trenutno nema zatvorenih igraca.");
	}
	else return Ovlascenje(playerid);
	return 1;
}

