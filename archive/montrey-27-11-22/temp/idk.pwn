YCMD:ban(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if(PI[playerid][Admin] < 2) return Ovlascenje(playerid);
	new id, razlog[40],string[256],banfile[80];
    if(sscanf(params,"us[40]",id,razlog)) return Koristi(playerid, "ban [ID] [Razlog]");
    if(!IsPlayerConnected(id)) return Error(playerid, "Pogresan ID.");
    if(PI[id][Admin] >= 6 || PI[id][Admin] >= 5) return Error(playerid, "Ne mozete banovati tog igraca.");
    if(PI[id][Admin] >= 5) return Error(playerid, "Ne mozete banovati tog igraca.");
    format(banfile, sizeof(banfile), BAN_FOLDER, Ime_Igraca(id));
	if(fexist(banfile)) return Error(playerid, "Igrac je vec banovan.");
	format(string,sizeof(string),"Banovali ste igraca %s sa razlogom: %s.", Ime_Igraca(id), razlog);
    SCM(playerid,CRVENA, string);
    format(string,sizeof(string),"Banovani ste od strane admina,razlog bana: %s.", razlog);
	SCM(id,CRVENA, string);
	format(string,sizeof(string),"{F23A0D}[BAN]: {FFFFFF}%s je banovan od strane admina, razlog: {FFFFFF}%s",Ime_Igraca(id),razlog);
	SCMTA(GRAD2,string);
	format(string,sizeof(string),"![WARNING]: Admin %s je banovao igraca %s | Razlog: %s!",Ime_Igraca(playerid),Ime_Igraca(id),razlog);
	StaffPoruka(COLOR_YELLOW,string);
	BanPlayer(id, razlog, Ime_Igraca(playerid));
	BanPL(id);
	PI[id][BanovanPuta]++;
    ServerS[sBans] ++;
    SacuvajServerStats();
    format(logstring, sizeof logstring, "| Igrac: %s | Admin: %s | Razlog: %s", Ime_Igraca(playerid),Ime_Igraca(id), razlog);
	UpisiLog(FILE_BAN, logstring);
