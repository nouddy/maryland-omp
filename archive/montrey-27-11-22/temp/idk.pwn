stock PostaviWanted(playerid, zlocin[], prijavio[])
{
	if(IsPlayerConnected(playerid))
	{
		SetPlayerWantedLevel_Ex(playerid, GetPlayerWantedLevel_Ex(playerid)+1); PI[playerid][TrazeniNivo] += 1;
		if(GetPlayerWantedLevel_Ex(playerid) > 6) { SetPlayerWantedLevel_Ex(playerid, 6); PI[playerid][TrazeniNivo] = 6; }
		format(nellys, sizeof(nellys), "[HQ]: Sve jedinice: Prijavio: %s", prijavio);
		FamilyPoruka(1, TEAM_BLUE_COLOR, nellys);
        FamilyPoruka(2, TEAM_BLUE_COLOR, nellys);
	    format(nellys, sizeof(nellys), "[HQ]: Zlocin: %s, Osumljiceni: %s [WL: %d]", zlocin, Ime_Igraca(playerid), GetPlayerWantedLevel_Ex(playerid));
		FamilyPoruka(1, TEAM_BLUE_COLOR, nellys);
        FamilyPoruka(2, TEAM_BLUE_COLOR, nellys);
		new string[256];
		format(string, sizeof(string), "Trenutni Wanted Level: %d", GetPlayerWantedLevel_Ex(playerid));
		SCM(playerid, COLOR_LIGHTRED, string);
		format(string, sizeof(string), "Pocinili ste zlocin: %s. Prijavio: %s.", zlocin, prijavio);
		SCM(playerid, COLOR_LIGHTRED, string);
		strmid(ZlocinInfo[playerid][zPrijavio], prijavio, 0, strlen(prijavio), 255);
		strmid(ZlocinInfo[playerid][zPocinio], Ime_Igraca(playerid), 0, strlen(Ime_Igraca(playerid)), 255);
		strmid(ZlocinInfo[playerid][zZlocin], zlocin, 0, strlen(zlocin), 255);
	}
	return 1;
}
