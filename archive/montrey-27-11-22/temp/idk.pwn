YCMD:napraviteritoriju(playerid, params[], help)
{
    if(PI[playerid][Admin] < 6)
    {
		Ovlascenje(playerid);
		return 1;
	}
	new tazonaid = SledecaZona();
    new team, vrsta;
    if(sscanf(params, "ii",team,vrsta))
	{
		SCM(playerid, -1, ""C_BELA"/napravizonu [Team] [VRSTA(0 - obicna | 1 - Glavna)]");
		SCM(playerid, -1, ""C_BELA"Timovi: 1.Vagos | 2.Surenos | 3.GSF | 4.Ballas");
		return 1;
	}
    if(team < 1 || team > 13) return Error(playerid, "Team ne moze biti manji od 1 i veci od 13!");
    if(vrsta < 0 || vrsta > 1) return Error(playerid, "Vrsta ne moze biti manja od 0 i veca od 1!");
	if(tazonaid >= MAX_ZONA) return Error(playerid, "Ne mozete napraviti vise od 200 teritorija!");
	if(team == 1) GZ[tazonaid][gTeam] = 6;
	else if(team == 2) GZ[tazonaid][gTeam] = 5;
	else if(team == 3) GZ[tazonaid][gTeam] = 4;
	else if(team == 4) GZ[tazonaid][gTeam] = 3;
	GZ[tazonaid][gGlavna] = vrsta;
	dodajezonu[playerid] = tazonaid;
	zonalvl[playerid] = 1;
	SCMF(playerid,-1,""SERVER"Zapoceli ste sa pravljenjem teritorije ID: %d",tazonaid);
    X_SCM(playerid, "Sada morate definisati regiju, definisete je sa slovom Y!");
    X_SCM(playerid, "Kada oznacite jednu tacku idite diagonalno na drugu tacku!");
    X_SCM(playerid, "JAKO JE BITNO DA ZONA IDE OD LEVO DIAGONALNO NA DESNO!!!");
    SacuvajZonu(tazonaid);
    return 1;
}
