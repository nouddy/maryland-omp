YCMD:hduty(playerid, params[],help)
{
    #pragma unused help
    #pragma unused params
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if(PI[playerid][GM] == 0) return Ovlascenje(playerid);
	if(PI[playerid][TrazeniNivo] >= 1) return Error(playerid, "Ne mozete koristiti ovu komandu jer imate wanted level.");
	if(IgracNaEventu[playerid] == 1) return Error(playerid, "Na eventu si,ne mozes to.");
	new stskin = PI[playerid][pSkin];
	if(HelperDuty[playerid] == 0)
	{
		format(nellys,sizeof(nellys),"(( {FFFFFF}Helper {48E31C}%s {FFFFFF}je na duznosti, koristi {48E31C}/pitaj {FFFFFF}ako mu zelis postaviti pitanje! {48E31C}))",Ime_Igraca(playerid));
		SCMTA(0x48E31CFF, nellys);
		HelperDuty[playerid] = 1;
		SetPlayerColor(playerid, 0x48E31CFF);
		g_SetPlayerArmour(playerid, 99.0);
		Update_Skin(playerid, 188);
		
		SetPlayerHealth_H(playerid, 99.0);
	}
	else if(HelperDuty[playerid] == 1)
	{
		g_SetPlayerArmour(playerid, 0);
		SetPlayerHealth_H(playerid, 99.0);
		Update_Skin(playerid, stskin);
		
		SetPlayerColor(playerid,TEAM_HIT_COLOR);
		format(nellys,sizeof(nellys),"(( {FFFFFF}Helper {48E31C}%s{FFFFFF} vise nije na duznosti. {48E31C}))",Ime_Igraca(playerid));
		SCMTA(0x48E31CFF,nellys);
		HelperDuty[playerid] = 0;
	}
	return 1;
}
YCMD:vpopravivozilo(playerid, params[],help)
{
    #pragma unused help
    #pragma unused params
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if(PI[playerid][pVIP] == 3)
	{
	    if(IgracNaEventu[playerid] == 1) return Error(playerid, "Na eventu si,ne mozes to.");
		if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid,-1, ""SERVERPLAVA"(MG:RPG) "C_BELA"Morate biti u vozilu.");
