stock stringContainsIP(string[])
{
    new
        dotCount
    ;
    for(new i; string[i] != EOS; ++i)
    {
        if(('0' <= string[i] <= '9') || string[i] == '.' || string[i] == ':')
        {
            if((string[i] == '.') && (string[i + 1] != '.') && ('0' <= string[i - 1] <= '9'))
            {
                ++dotCount;
            }
            continue;
        }
    }
    return (dotCount > 2);
}

stock getPlayerNearestVehicle(playerID)
{
	new
		nearestVehicle = INVALID_VEHICLE_ID,
		Float:distance = 8.0,
		Float:smallestDistance = 8.0,
		Float:playerPos[3];

	GetPlayerPos(playerID, playerPos[0], playerPos[1], playerPos[2]);

	for(new i = 1; i <= MAX_VEHICLES; i++)
	{
	    if (!GetVehicleModel(i) || !IsVehicleStreamedIn(i, playerID)) continue;

		distance = GetVehicleDistanceFromPoint(i, playerPos[0], playerPos[1], playerPos[2]);

		if (distance <= 8.0 && distance <= smallestDistance)
		{
			smallestDistance = distance;
			nearestVehicle = i;
		}
	}
	return nearestVehicle;
}

