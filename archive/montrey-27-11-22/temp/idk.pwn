	if(PI[playerid][KgPeceneRibe] > 0)
 	{

		if(gettime() < jeoribu[playerid]) return SCM(playerid, GRAD2, "Komandu mozete koristiti svakih 30 sekundi!");
    	jeoribu[playerid] = gettime() + 30;
	    new Float:hp, Float:nhp;
	    GetPlayerHealth(playerid, hp);
	   	nhp = floatadd(hp, 10.0);
	    SetPlayerHealth_H(playerid, nhp);
	    if(nhp > 100.0) SetPlayerHealth_H(playerid, 100);
