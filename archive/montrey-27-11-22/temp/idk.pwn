YCMD:makeadmin(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if(PI[playerid][Admin] >= 6 || IsPlayerAdmin(playerid))
	{
		new player, nivo;
		if(sscanf(params, "ud", player, nivo)) return Koristi(playerid, "makeadmin [ID/Dio Imena] [Level (1-6)]");
		if(!IsPlayerConnected(player)) return SCM(playerid, -1,"{FF6600}(MG:RPG) "C_BELA"Pogresan ID.");
		if(nivo < 0 || nivo > 7) return SCM(playerid, BELA, "{FF6600}(MG:RPG) {FFFFFF}Admin Level ne moze biti manji od 0 i veci od 6.");
		if(PI[player][GM] > 0) return Error(playerid, "Taj igrac ima poziciju helpera,prvo je skinite.");
		if(PI[player][pVIP] > 0) return Error(playerid, "Taj igrac ima poziciju vipa,prvo je skinite.");
		if(PI[player][Promoter] > 0) return Error(playerid, "Taj igrac ima poziciju promotera,prvo je skinite.");
		if(nivo == 0)
	    {
	        format(nellys, sizeof(nellys), "Skinuta vam je admin pozicija od strane %s.", Ime_Igraca(playerid));
	        SCM(player, SVETLOPLAVA, nellys);
	        SCMF(playerid, SVETLOPLAVA, "      Promovisali ste %s na admin level 0.", Ime_Igraca(player));
	 		new pom;
			pom = PI[player][pAdminID];
			strmid(AdminInfo[pom][aAdmin], "Niko", 0, strlen("Niko"), 255);
		    AdminInfo[pom][aRank] = 0;
			AdminInfo[pom][aNeaktivnost] = 0;
		    SacuvajAdmine();
			PI[player][Admin] = 0;
			PI[player][pAdminID] = 0;
			gChatActive[playerid] = false;
			SavePlayer(player);
	    }
	    else
	    {
			if(PI[player][pAdminID] > 0)
			{
			    new paID = PI[player][pAdminID];
			    AdminInfo[paID][aRank] = nivo;
				AdminInfo[paID][aNeaktivnost] = 0;
			    SacuvajAdmine();
			    PI[player][Admin] = nivo;
			    printf("AdmYCMD: %s je promovisao %s na admin level %d.", Ime_Igraca(playerid), Ime_Igraca(player), nivo);
				format(nellys, 128, "Promovisali ste %s na admin level %d.", Ime_Igraca(player), nivo);
	    		SCM(playerid, SVETLOPLAVA, nellys);
			    format(nellys, 128, "Promovisani ste na admin level %d od strane %s.", nivo, Ime_Igraca(playerid));
			    SCM(player, SVETLOPLAVA, nellys);
			    gChatActive[playerid] = true;
			    SavePlayer(player);
				format(nellys, sizeof(nellys), "![WARNING]: Admin %s je promovisan na admin level %d od strane: %s.", Ime_Igraca(player),nivo, Ime_Igraca(playerid));
				AdminPoruka(NARANDZASTA, nellys);
			    return 1;
			}
		    for(new n = 1; n < MAX_ADMINS; n++)
		    {
		        if(!strcmp(AdminInfo[n][aAdmin], "Niko", true))
		    	{
					strmid(AdminInfo[n][aAdmin], Ime_Igraca(player), 0, strlen(Ime_Igraca(player)), 255);
					AdminInfo[n][aRank] = nivo;
					AdminInfo[n][aNeaktivnost] = 0;
					SacuvajAdmine();
					PI[player][pAdminID] = n;
					format(nellys, 128, "Promovisali ste %s na admin level %d.", Ime_Igraca(player), nivo);
			    	SCM(playerid, SVETLOPLAVA, nellys);
					format(nellys, 128, "Promovisani ste na admin level %d od strane %s.", nivo, Ime_Igraca(playerid));
					SCM(player, SVETLOPLAVA, nellys);
					gChatActive[playerid] = true;
                    PI[player][Admin] = nivo;
					SavePlayer(player);
					format(nellys, sizeof(nellys), "![WARNING]: Admin %s je promovisan na admin level %d od strane: %s.", Ime_Igraca(player),nivo, Ime_Igraca(playerid));
					AdminPoruka(NARANDZASTA, nellys);
					return 1;
				}
			}
  			return Error(playerid, "Svi admin slotovi su popunjeni.");
		}
	}
	else
	{
	    Ovlascenje(playerid);
	}
	return 1;
}
