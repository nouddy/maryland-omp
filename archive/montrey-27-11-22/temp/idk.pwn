	new tmpsecond;
	gettime(tmphour, tmpminute, tmpsecond);
	if ((tmphour > ghour) || (tmphour == 0 && ghour == 23))
	{
		ghour = tmphour;
		SetWorldTime(tmphour);
		FixHour(tmphour);
		new vrijemes[126];
		format(vrijemes, sizeof(vrijemes), "|| PAYDAY || Sada je tacno {07C1F5}%d:00h",tmphour);
