	if(PI[playerid][TrazeniNivo] <= 0) return KillTimer_H(smanjiwl[playerid]);
	if(PI[playerid][Zatvor] > 0 || GetPlayerVirtualWorld(playerid) > 0 || GetPlayerInterior(playerid) > 0) return 1;
