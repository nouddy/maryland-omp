YCMD:obijbravu(playerid, params[],help)
{
    #pragma unused help
    #pragma unused params
	if(PI[playerid][zZice] <= 0) return Error(playerid, "Nemate potreban alat za obijanje.");
	if(!IsPlayerInAnyVehicle(playerid)) return Error(playerid, "Nisi u vozilu.");
	new lspd = 0;
	foreach(Player, i)
    {
    	if(PI[i][Clan] == 1 || PI[i][Lider] == 1) lspd++;
	}
	if(lspd == 0) return Error(playerid, "Nema online pripadnika policije ((Nije RP pljackati bez njih))!");
    new vehicleid, found;
    for(new i = 1; i <= MAX_VEHICLES; i++)
    {
        GetVehiclePos(i, X, Y, Z);
        if(IsPlayerInRangeOfPoint(playerid, 3.0, X, Y, Z)) {
            vehicleid = i;
            found = 1;
            break;
        }
        else continue;
    }
    if(!found)
	{
        Error(playerid, "Nisi blizu vozila.");
        return 1;
    }
	new bool:founds;
	for(new i = 0; i < sizeof(VoziloInfo); i++)
	{
		if(vehicleid == VoziloInfo[i][vID])
		{
			founds = true;
			break;
		}
	}
	if(!founds) return Error(playerid, "Ne mozete obiti bravu na ovom vozilu!");

	if(PI[playerid][zZice] > 0) PI[playerid][zZice]--;
	new rnd = random(4);
	switch(rnd)
	{
		case 0, 1, 2:
		{
			SCM(playerid, GRAD2, "Uspesno ste obili bravu i pokrenuli vozilo.");
			new engine, lights, alarm, doors, bonnet, boot, objective;
			GetVehicleParamsEx(GetPlayerVehicleID(playerid), engine, lights, alarm, doors, bonnet, boot, objective);
			SetVehicleParamsEx(GetPlayerVehicleID(playerid), 1, lights, alarm, doors, bonnet, boot, objective);
         	format(nellys, sizeof(nellys), "* Zice vozila se prespojile,motor upaljen. (( %s ))", Ime_Igraca(playerid));
      		ProxDetector(10.0, playerid, nellys, PROX,PROX,PROX,PROX,PROX);
		}
		default:
		{
			SCM(playerid, ZUTA, "Niste uspeli da pokrenete motor vozila.");
		}
	}
	SavePlayer(playerid);
	return 1;
}
