function PayDay(i)
{
		if(!IsPlayerConnected(i) || UlogovanProvera[i] == 0) return 1;
		if(PI[i][TrazeniNivo] > 0) PI[i][TrazeniNivo]--;
		if(PI[i][TrazeniNivo] > 0) SCMF(i, SVETLOCRVENA, " * Vas trazeni nivo je sada: {FFFFFF}%d. {FF6347}Pazite se policije!", PI[i][TrazeniNivo]);
	 	else if(PI[i][TrazeniNivo] == 0)
		{
			SCM(i, SVETLOCRVENA, " * Vas trazeni nivo je nestao. Vise nema opasnosti od policije, mozete se vratiti u grad.");
		}
		SetPlayerWantedLevel_Ex(i, PI[i][TrazeniNivo]);
		new stariiznos = PI[i][Banka];
		if(PI[i][pRent] != -1)
		{
			if(PI[i][Banka] < KucaInfo[PI[i][pRent]][kCenaRenta])
			{
				PI[i][pRent] = -1;
				PI[i][pProveraSpawna] = SPAWN_NORMAL;
				SCM(i, COLOR_GRAD2, "Niste imali da platiti kiriju za kucu, iseljeni ste.");
			}
			PI[i][Banka] -= KucaInfo[PI[i][pRent]][kCenaRenta];
		}
		if(PI[i][Banka] >= PI[i][pIznosRate] && PI[i][pPreostaloZaOtplatu] > 0)
		{
		    if(PI[i][pPreostaloZaOtplatu] < PI[i][pIznosRate])
		    {
		        PI[i][pIznosRate] = PI[i][pPreostaloZaOtplatu];
			}
		    PI[i][pPreostaloZaOtplatu] -= PI[i][pIznosRate];
		    PI[i][Banka] -= PI[i][pIznosRate];
		    if(PI[i][pPreostaloZaOtplatu] < 0) PI[i][pPreostaloZaOtplatu] = 0;
		    if(PI[i][pPreostaloZaOtplatu] <= 0)
			{
				PI[i][pIznosKredita] = 0; PI[i][pIznosRate] = 0;
			}
	 	}
	 	if(PI[i][pPayDayPoeni] < 35)
		{
			SCM(i, SVETLOCRVENA, " * Niste igrali dovoljno dugo da dobijete platu!");
			hDobioPay[i] = 1;
			return 1;
		}
		if(PI[i][Clan] > 0) PI[i][pTimUgovor]++;
		if(PI[i][pVIP] > 0)
		{
			PI[i][pVIPBodovi] += 2;
			SCM_SGC(i, "Posto ste VIP dobili ste +2 VIP boda, uzivajte u igri!");
		}
		if(PI[i][pKaznaUgovor] > 0)
		{
			PI[i][pKaznaUgovor]--;
			if(PI[i][pKaznaUgovor] == 0) SCM(i, SVETLOPLAVA, "Vasa zabrana ulaska u organizaciju/mafiju/bandu je prosla!");
		}
		if(PI[i][Lider] > 0)
		{
			SCM_SGC(i," Ti si lider organizacije i dobio si 500$ na bankovni racun!");
		 	PI[i][Banka] += 500;
		}
		if(PI[i][pPosaoUgovor] > 0) { PI[i][pPosaoUgovor]--; }
	    new iznosplate = 0, osnova = 0, iskustvo = 0, prihod = 0, rashod = 0, struja = 0, voda = 0, smece = 0, grijanje = 0, porez = 0, kucanajam = 0;
		if(PI[i][pPosao] > 0)
		{
			osnova = randomEx(5,10);
	  	}
		iznosplate = osnova*PI[i][pNivo];
		PI[i][SatiIgre]++;
		PI[i][pRespekti]++;
		iskustvo = (PI[i][pNivo] +1) * 4;
		if(duplirespect == 1) PI[i][pRespekti]++;
		if(PI[i][pVIP] == 3) PI[i][pRespekti] ++;
		if(PI[i][Kuca] != -1)
		{

			if(!strcmp(KucaInfo[PI[i][Kuca]][kVrsta], "Mala kuca", false)) 	{	struja += (10 + random(5)); voda += (5 + random(7)); porez += 10; smece += 5; grijanje += 5; 			}
			else if(!strcmp(KucaInfo[PI[i][Kuca]][kVrsta], "Srednja kuca", false)) 	{   struja += (12 + random(5)); voda += (5 + random(7)); porez += 15; smece += 5;  grijanje += 7; 		}
			else if(!strcmp(KucaInfo[PI[i][Kuca]][kVrsta], "Velika kuca", false))  	{    struja += (13 + random(5)); voda += (5 + random(7)); porez += 15; smece += 5; grijanje += 9;		}
			else if(!strcmp(KucaInfo[PI[i][Kuca]][kVrsta], "Vila", false))         	{    struja += (15 + random(5)); voda += (5 + random(10)); porez += 20; smece += 5; grijanje += 12;	}
		}
		if(PI[i][pPosedovanjeStana] != -1) { struja += (10 + random(5)); voda += (5 + random(5)); porez += 5; smece += 5; grijanje += 5; }
		if(PI[i][pPosedovanjeVikendice] != -1) { struja += (5 + random(15)); voda += (10 + random(10)); porez += 10; smece += 10; grijanje += 5; }
		if(PI[i][pPosedovanjeFirme] != -1) { porez += 250; }
		if(PI[i][pRent] == -1) kucanajam = 0;
		else if(PI[i][pRent] != -1) kucanajam = KucaInfo[PI[i][pRent]][kCenaRenta];
		new strp1[300],strp2[450],strp[600];
		if(PI[i][pRespekti] >= iskustvo)
		{
			PI[i][pRespekti] = 0;
			PI[i][pNivo]++;
			SetPlayerScore(i, PI[i][pNivo]);
			SCM(i, ZUTA, "'MOJA BANKA' BANKA: Postovani korisnice,transfer vase plate je uspesno izvrsen na vas ziro racun.");
			format(strp1,sizeof(strp1), "{00FFFF}Plata je stigla na vas racun\n\n\n{00FFFF}Plata: "C_BELA"%d$\n\n"ORANGE"Iznos kredita: "C_ZELENA"%d$\n{00FFFF}Iznos rate: "C_ZELENA"%d$\n{00FFFF}Za otplatu: {FFFFFF}%d%\n\n{00FFFF}Porez na imovinu: "C_BELA"%d$\n", iznosplate, PI[i][pIznosKredita], PI[i][pIznosRate], PI[i][pPreostaloZaOtplatu],porez);
	        format(strp2,sizeof(strp2), "{00FFFF}Struja: {00FFFF}%d$\n{00FFFF}Voda: "C_BELA"%d$\n{00FFFF}Smece: "C_BELA"%d$\n{00FFFF}Grejanje: "C_BELA"%d$\n{00FFFF}Iznajmljena kuca: "C_BELA"%d$\n\n{00FFFF}Staro stanje: "C_BELA"%d$\n{00FFFF}Novo stanje: "C_BELA"%d$\n\n{6DD91A}Cestitamo, presli ste na sledeci nivo!", struja, voda, smece,grijanje,kucanajam, stariiznos,PI[i][Banka]+iznosplate);
			new nxtlevel[56];
	        format(nxtlevel, 56, "~g~LEVEL UP~n~~w~Sada si nivo %d", PI[i][pNivo]);
	        GameTextForPlayer(i, nxtlevel, 5000, 1);
		}
		else
		{
	    	SCM(i, ZUTA, "'MOJA BANKA' BANKA: Postovani korisnice,transfer vase plate je uspesno izvrsen na vas ziro racun.");
			format(strp1,sizeof(strp1), "{00FFFF}Plata je stigla na vas racun\n\n\n{00FFFF}Plata: "C_BELA"%d$\n\n{00FFFF}Iznos kredita: "C_BELA"%d$\n{00FFFF}Rata: "C_BELA"%d$\n{00FFFF}Za otplatu: "C_BELA"%d%\n\n", iznosplate, PI[i][pIznosKredita], PI[i][pIznosRate], PI[i][pPreostaloZaOtplatu]);
	        format(strp2,sizeof(strp2), "{00FFFF}Porez na imovinu: "C_BELA"%d$\n{00FFFF}Struja: {00FFFF}%d$\n{00FFFF}Voda: "C_BELA"%d$\n{00FFFF}Smece: "C_BELA"%d$\n{00FFFF}Grejanje: "C_BELA"%d$\n{00FFFF}Iznajmljena kuca: "C_BELA"%d$\n\n{00FFFF}Staro stanje: "C_BELA"%d$\n{00FFFF}Novo stanje: "C_BELA"%d$", porez, struja, voda, smece,grijanje,kucanajam, stariiznos,PI[i][Banka]+iznosplate);
		}
		format(strp, sizeof(strp), "%s%s", strp1, strp2);
		SPD(i, DIALOG_PLATA, DIALOG_STYLE_MSGBOX, ""SERVERPLAVA"Plata", strp, "Uredu", "Izadji");
		prihod = iznosplate;
		rashod = struja + voda + porez + smece + grijanje;
		PI[i][Banka] += prihod;
		PI[i][Banka] -= rashod;
		PI[i][pPayDayPoeni] = 0;
		hDobioPay[i] = 1;
		SavePlayer(i);
    return 1;
}
