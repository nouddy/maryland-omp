public OnPlayerEnterCheckpoint(playerid)
{
    if(pikolidebug == 1) printf("[Pikoli-DEBUG] OnPlayerEnterCheckpoint %d",playerid);
    new CID = GetPlayerVehicleID(playerid);
    if(PosaoGPS[playerid] == 1) return DisablePlayerCheckpoint(playerid), PosaoGPS[playerid] = 0, GameTextForPlayer(playerid, "~y~stigli ste na odrediste", 5000, 3);
    if(GPSAktivan[playerid] == 1)
	{
		h_DisablePlayerCheckpoint(playerid);
		DestroyDynamicObject(GPSObjekat[playerid]);
		GameTextForPlayer(playerid, "~g~stigli ste na datu lokaciju!", 3000, 1);
  		GPSAktivan[playerid] = 0;
		GPSTD2(playerid, 0, "_");
	}
	if(hPrihvatio[playerid] != -1)
	{
		h_DisablePlayerCheckpoint(playerid);
		GameTextForPlayer(playerid, "~g~Stigli ste na odrediste", 2500, 1);
	    hPrihvatio[playerid] = -1;
	}
	// ----------------------------- mehanicar ---------------------------------
	if(IsPlayerInRangeOfPoint(playerid, 5.0, 2081.1367,-2019.7092,13.5469))
	{
	    if(PosaoOprema[playerid] == 1 && PI[playerid][pPosao] == POSAO_MEHANICAR && CID >= vmehanicar[0] && CID <= vmehanicar[3] && GetPlayerState(playerid) == 2)
	    {
	        if(PI[playerid][MehanicarOdvoz] != 0)
	        {
	            DisablePlayerCheckpoint(playerid);
	            format(nellys, 128, "Morate sacekati jos %s do sledeceg odvoza.", KonvertujVreme(PI[playerid][MehanicarOdvoz]));
	    		Error(playerid, nellys);
	    		return 1;
			}
			if(IsTrailerAttachedToVehicle(CID))
			{
				DisablePlayerCheckpoint(playerid);
		     	new plata = 4500;
		       	new bonus = randomEx(30,50);
	            SCMF(playerid, POSAOBOJA, "[POSAO]: {FFFFFF}Zaradili ste $%d od ovog automobila!", plata+bonus);
	           	Plata(playerid, plata, bonus);
	       		FirmaNovac(25, plata/2);
		        PI[playerid][MehanicarOdvoz] = 1800;
		        SetVehicleToRespawn(GetVehicleTrailer(GetPlayerVehicleID(playerid)));
			}
			else return Error(playerid, "Nemate zakacen auto!");
		}
	}
	if(hSmetlar[playerid] == true && !IsPlayerInAnyVehicle(playerid))
	{
	    DisablePlayerCheckpoint(playerid);
	    DestroyPlayerObject(playerid,hSmece[playerid]);

		SetPlayerAttachedObject(playerid, 7, 1265, 6, 0.35, 0.0, -0.22, -10.0, -90.0);
		SCM(playerid, POSAOBOJA, "[POSAO]: {FFFFFF}Stavite smece u kamion! Priblizite se kamionu i upisite /ubacismece!");
	}
	if(IsPlayerInRangeOfPoint(playerid, 5.0, -59.9487,-1579.0037,2.6107))
    {
        if(PI[playerid][pPosao] == POSAO_KOMUNALAC && CID >= vkomunalno[0] && CID <= vkomunalno[4] && GetPlayerState(playerid) == 2)
        {
            TogglePlayerControllable(playerid, 0);
            dostavaistovar[playerid] = SetTimerEx("Istovar", 10000, false, "d", playerid);
            GameTextForPlayer(playerid, "~w~Istovar smeca", 5000, 1);
			DisablePlayerCheckpoint(playerid);
            new plata = 4500;
            new bonus = randomEx(10, 30);
			SCM(playerid, POSAOBOJA, "[POSAO]: {FFFFFF}Uspesno ste zavrsili sakupljanje smeca.");
			if(PI[playerid][pPosedovanjeFirme] == 28) { Plata(playerid, plata/2, bonus/2); SCM(playerid, ZUTA, "Posto ste vlasnik firme dobijate pola plate!"); }
			else { Plata(playerid, plata, bonus); }
			FirmaNovac(28, plata/2);
			hVreca[playerid] = 0;
			PosaoAktivan[playerid] = 0;
        }
    }
	// ============================================= <[ RONILAC ]> =============================================
	if(IsPlayerInRangeOfPoint(playerid, 5.0, -43.1962,-2166.3347,-0.3239))
	{
	    if(RonilacOprema[playerid] == 1 && PI[playerid][pPosao] == POSAO_RONILAC && CID >= Ronilac[1] && CID <= Ronilac[5] && GetPlayerState(playerid) == 2 && hRonilac[playerid] == 1)
	    {
	        h_DisablePlayerCheckpoint(playerid);
	        hRonilac[playerid] = 2;
	        SCM(playerid, ZUTA, "POSAO | Napustite plovilo i zaronite te nadjite zlato u napustenom brodu.");
		}
	}
	if(IsPlayerInRangeOfPoint(playerid, 5.0, 386.0558,-1917.7455,2.0210))
	{
	    if(RonilacOprema[playerid] == 1 && PI[playerid][pPosao] == POSAO_RONILAC && hRonilac[playerid] == 3)
	    {
	        h_DisablePlayerCheckpoint(playerid);
	        hRonilac[playerid] = 0;
	        new DonioZlato;
	        DonioZlato = 20+randomEx(10,30);
        	hUkupnoZlata[playerid] = hUkupnoZlata[playerid] + DonioZlato;
        	format(nellys, sizeof(nellys), "POSAO | Donjeli ste zlato tezine %d g | Ukupno: %d g, kad vratite opremu dobit cete zaradjeni novac.", DonioZlato, hUkupnoZlata[playerid]);
    		SendClientMessage(playerid, ZUTA, nellys);
    		DonioZlato = 0;
    		RemovePlayerAttachedObject(playerid, 7);
		}
	}
    // -------------------------------------------------------------------------
    if(IsPlayerInRangeOfPoint(playerid, 5.0, -2167.2078,-209.3802,35.3203))
    {
        if(PI[playerid][pPosao] == POSAO_DBRASNA && CID >= vbrasno[0] && CID <= vbrasno[7] && GetPlayerState(playerid) == 2 && ZapoceoPrenos[playerid] == true)
        {
			SCM(playerid, POSAOBOJA, "[POSAO]: {FFFFFF}Sada pridjite pozadi,i levim klikom misa uzimajte jedan po jedan dzak i istovarite.");
			h_SetPlayerCheckpoint(playerid, -2187.1611,-209.2763,36.5156, 3.0);
			IstovaraBrasno[playerid] = 1;
	   }
    }
    if(IsPlayerInRangeOfPoint(playerid, 3.0, -2187.1611,-209.2763,36.5156))
    {
        if(PI[playerid][pPosao] == POSAO_DBRASNA && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && ZapoceoPrenos[playerid] == true && NosiBrasno[playerid] == true)
        {
            NosiBrasno[playerid] = false;
            SetPlayerSpecialAction(playerid, SPECIAL_ACTION_NONE);
			RemovePlayerAttachedObject(playerid, 0);
			GameTextForPlayer(playerid, "~w~dzak istovaren", 4000, 3);
			IstovarenoDzakova[playerid]++;
			if(IstovarenoDzakova[playerid] == 7)
			{
			    IstovaraBrasno[playerid] = 0;
				h_SetPlayerCheckpoint(playerid, 1096.7185,-1215.9500,17.8047, 3.0);
				SCM(playerid, POSAOBOJA, "[POSAO]: {FFFFFF}Sada idite do oznacenog mesta na mapi po platu.");
			}
	   	}
    }
    if(IsPlayerInRangeOfPoint(playerid, 3.0, 1096.7185,-1215.9500,17.8047))
    {
        if(PI[playerid][pPosao] == POSAO_DBRASNA && CID >= vbrasno[0] && CID <= vbrasno[7] && GetPlayerState(playerid) == 2 && ZapoceoPrenos[playerid] == true)
        {
            ZapoceoPrenos[playerid] = false;
            UtovarBrasna[playerid] = 0;
			IstovaraBrasno[playerid] = 0;
			IstovarenoDzakova[playerid] = 0;
            SetVehicleToRespawn(GetPlayerVehicleID(playerid));
            DisablePlayerCheckpoint(playerid);
            new plata = 3500;
            new bonus = randomExx(10, 40);
			SCM(playerid, POSAOBOJA, "[POSAO]: {FFFFFF}Uspesno ste zavrsili dostavu brasna.");
			if(PI[playerid][pPosedovanjeFirme] == 65) { Plata(playerid, plata/2, bonus/2); SCM(playerid, ZUTA, "Posto ste vlasnik firme dobijate pola plate!"); }
			else { Plata(playerid, plata, bonus); }
			FirmaNovac(65, plata/2);
	   }
    }
    // ---------------------- dostavljac auto dijelova -------------------------
    if(IsPlayerInRangeOfPoint(playerid, 5.0, 1947.0750,-2034.0608,13.5469))
    {
        if(PI[playerid][pPosao] == POSAO_DDELOVA && CID >= ddelova[0] && CID <= ddelova[4] && GetPlayerState(playerid) == 2 && ZapoceoPrenosD[playerid] == true)
        {
			SCM(playerid, POSAOBOJA, "[POSAO]: {FFFFFF}Sada pridjite pozadi,i levim klikom misa uzimajte jednu po jednu kutiju i istovarite.");
			h_SetPlayerCheckpoint(playerid, 1936.2052,-2032.7455,13.5759, 3.0);
			IstovaraDelove[playerid] = 1;
	   }
    }
