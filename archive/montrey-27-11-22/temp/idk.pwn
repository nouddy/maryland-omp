	if(sscanf(params, "uiii", igrac, droga, kolicina, cijena))
	{
	    Koristi(playerid, "prodajdrogu [ID/Dio imena] [Droga(1-3)] [Kolicina] [Cena]");
	    SCM(playerid, BELA, "DROGE: 1) Kokain 2) Cannabis" );
	    return 1;
	}
	if(PI[igrac][pNivo] <= 1) return Error(playerid,"Ne mozete prodati igracu koji je nivo 1.");
	if(cijena < 1) return Error(playerid, "Cena ne moze biti u tom formatu. (Min 1)");
	if(kolicina < 1) return Error(playerid, "Kolicina ne moze biti u tom formatu. (Min 1)");
	if(droga == 1)
	{
		GetPlayerPos(playerid, Position[0], Position[1], Position[2]);
		if(!IsPlayerInRangeOfPoint(igrac, 3.0, Position[0], Position[1], Position[2])) return Error(playerid, "Igrac nije blizu vas." );
		if(igrac == playerid) return Error(playerid, "Ne mozes sam sebi prodat kokain.");
		if(cijena > PI[igrac][pMoney]) return Error(playerid, "Igrac nema novca za toliko kokaina.");
		if(kolicina > PI[playerid][Kokain]) return Error(playerid, "Nemas toliko kokaina.");
		KolicinaDroge[igrac] = kolicina;
		CenaDroge[igrac] = cijena;
		vrstadr[igrac] = droga;
		ProdajaDroge[igrac] = playerid;
		SCMF(igrac, SVETLOPLAVA, "* %s vam je ponudio %d grama kokaina po ceni od $%d. (/prihvatidrogu /odbijdrogu)", Ime_Igraca(playerid), kolicina, cijena);
		SCMF(playerid, SVETLOPLAVA, "* Ponudili ste igracu %s %d grama kokaina po ceni od $%d", Ime_Igraca(igrac), kolicina, cijena);
	}
	else if(droga == 2)
	{
		GetPlayerPos(playerid, Position[0], Position[1], Position[2]);
		if(!IsPlayerInRangeOfPoint(igrac, 3.0, Position[0], Position[1], Position[2])) return Error(playerid, "Igrac nije blizu vas.");
		if(igrac == playerid) return Error(playerid, "Ne mozes sam sebi prodat Cannabis.");
		if(cijena > PI[igrac][pMoney]) return Error(playerid, "Igrac nema novca za toliko Cannabis-a.");
		if(kolicina > PI[playerid][Cannabis]) return Error(playerid, "Nemas toliko Cannabis-a.");
		KolicinaDroge[igrac] = kolicina;
		CenaDroge[igrac] = cijena;
		vrstadr[igrac] = droga;
		ProdajaDroge[igrac] = playerid;
		SCMF(igrac, SVETLOPLAVA, "* %s vam je ponudio %d grama Cannabis-a po ceni od $%d.", Ime_Igraca(playerid), kolicina, cijena);
		SCMF(playerid, SVETLOPLAVA, "* Ponudili ste igracu %s %d grama Cannabis-a po ceni od $%d", Ime_Igraca(igrac), kolicina, cijena);
	}
	return 1;
}
YCMD:prihvatidrogu(playerid, params[],help)
{
	if(ProdajaDroge[playerid] == -1) return Error(playerid, "Niko ti nije ponudio da kupis drogu.");
	if(vrstadr[playerid] == 2)
	{
		PI[playerid][Kokain] += KolicinaDroge[playerid];
		PI[ProdajaDroge[playerid]][Kokain] -= KolicinaDroge[playerid];
		g_NovacMinus(playerid, CenaDroge[playerid]);
		g_NovacPlus(ProdajaDroge[playerid], CenaDroge[playerid]);
		SCMF(playerid, SVETLOPLAVA, "* Kupili ste %d grama kokaina od %s", KolicinaDroge[playerid], Ime_Igraca(ProdajaDroge[playerid]));
		SCMF(ProdajaDroge[playerid], SVETLOPLAVA, "* Prodali ste %d grama kokaina %s-u", KolicinaDroge[playerid], Ime_Igraca(playerid));
		KolicinaDroge[playerid] = 0;
		CenaDroge[playerid] = 0;
		ProdajaDroge[playerid] = -1;
		vrstadr[playerid] = 0;
	}
	else if(vrstadr[playerid] == 3)
	{
		PI[playerid][Cannabis] += KolicinaDroge[playerid];
		PI[ProdajaDroge[playerid]][Cannabis] -= KolicinaDroge[playerid];
		g_NovacMinus(playerid, CenaDroge[playerid]);
		g_NovacPlus(ProdajaDroge[playerid], CenaDroge[playerid]);
		SCMF(playerid, SVETLOPLAVA, "Kupili ste %d grama Cannabis-a od %s.", KolicinaDroge[playerid], Ime_Igraca(ProdajaDroge[playerid]));
		SCMF(ProdajaDroge[playerid], SVETLOPLAVA, "Prodali ste %d grama Cannabis-a %s-u", KolicinaDroge[playerid], Ime_Igraca(playerid));
		KolicinaDroge[playerid] = 0;
		CenaDroge[playerid] = 0;
		ProdajaDroge[playerid] = -1;
		vrstadr[playerid] = 0;
	}
	return 1;
}
YCMD:odbijdrogu(playerid, params[],help)
{
	if(ProdajaDroge[playerid] == -1) return Error(playerid, "Niko ti nije ponudio da kupis drogu.");
	SCMF(playerid, SVETLOPLAVA, "Odbili ste kupovinu droge od %s", Ime_Igraca(ProdajaDroge[playerid]));
