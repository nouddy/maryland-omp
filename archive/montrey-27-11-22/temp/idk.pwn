	if(!IsPlayerInRangeOfPoint(playerid, 3.0, 2487.36, 2440.15, 54.95)) return SCM(playerid,-1,"Nisi na mjestu za upis");
	if(PI[playerid][pDiploma] == 1) return SCM(playerid,LJUBICASTA,"Sektretarica kaze: Vec imas diplomu");
	if(PI[playerid][pUpisan] == 1)  return SCM(playerid,LJUBICASTA,"Sektretarica kaze: Vec si upisan u ovu skolu");
	if(GetPlayerMoney(playerid) < 20)       return SCM(playerid,LJUBICASTA,"Sektretarica kaze: Nemas 20$");
	g_NovacMinus(playerid, 20);
	PI[playerid][pUpisan] = 1;
	new string[128];
	format(string,sizeof(string),"Sekretarica kaze: Cestitamo gospodine %s! Upisali ste se u RP skolu! Zelimo vam uspjesnu skolsku godinu!",Ime_Igraca(playerid));
	SCM(playerid, LJUBICASTA,string);
	SCM(playerid, LJUBICASTA, "Sekretarica kaze: Sada udjite u ucionicu za nastavu i mozete da pocnete vec sa nastavom.");
	return 1;
}
