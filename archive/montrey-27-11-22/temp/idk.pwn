YCMD:smece(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
    if(PI[playerid][pPosao] != POSAO_KOMUNALAC) return Error(playerid, "Nisi zaposlen kao komunalac!");
	if(GetPlayerVehicleID(playerid) < vkomunalno[0] || GetPlayerVehicleID(playerid) > vkomunalno[4]) return Error(playerid, "Ne nalazite se u kamionu za smece!");
	if(hSmetlar[playerid] == true) return Error(playerid, "Vec ste zapoceli posao!");
	if(!PosaoOprema[playerid])
	{
		Error(playerid, "Niste obukli opremu.");
		return 1;
	}
	hSmetlar[playerid] = true;
    SCM(playerid, POSAOBOJA, "[POSAO]: {FFFFFF}Zapoceli ste sakupljanje smeca.");
    SCM(playerid, POSAOBOJA, "[POSAO]: {FFFFFF}Na checkpoint ne idete kamionom,vec rukama kupite smece.");
	new rand = random(sizeof(SmeceCP));
	SetPlayerCheckpoint(playerid, SmeceCP[rand][0], SmeceCP[rand][1], SmeceCP[rand][2], 4.0);
	hSmece[playerid] = CreatePlayerObject(playerid,1265, SmeceCP[rand][0], SmeceCP[rand][1], SmeceCP[rand][2], 0.0, 0.0, 0.0);
	PosaoAktivan[playerid] = 1;
	return 1;
}
YCMD:ubacismece(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
    if(PI[playerid][pPosao] != POSAO_KOMUNALAC) return Error(playerid, "Nisi zaposlen kao komunalac.");
    if(hSmetlar[playerid] == false) return Error(playerid, "Nisi ni poceli raditi!!" );
    if(!IsPlayerAttachedObjectSlotUsed(playerid, 7)) return Error(playerid, "Nemate smece u rukama kako bi ste ga ubacili u kamion!");
	GetVehiclePos(GetClosestVehicle(playerid), X, Y, Z);
	if(!IsPlayerInRangeOfPoint(playerid, 6.0, X, Y, Z)) return Error(playerid, "Niste blizu smetlarskog kamiona!");
	if(GetVehicleModel(GetClosestVehicle(playerid)) != 408) return Error(playerid, "Niste blizu smetlarskog kamiona!");
	RemovePlayerAttachedObject(playerid, 7);
	hVreca[playerid] += 1;
	if(hVreca[playerid] < 5)
	{
		new rand = random(sizeof(SmeceCP));
		SetPlayerCheckpoint(playerid, SmeceCP[rand][0], SmeceCP[rand][1], SmeceCP[rand][2], 4.0);
