public OnModelSelectionResponse(playerid, extraid, index, modelid, response)
{
  	if ((response) && (extraid == MODEL_SELECTION_WHEELS))
	{
        new vehicleid = GetPlayerVehicleID(playerid);

		if (!IsPlayerInAnyVehicle(playerid) || !IsDoorVehicle(vehicleid))
	    	return 0;

	    AddVehicleComponent(vehicleid, modelid);
	    SCMF(playerid, BELA, ""SERVERPLAVA"(MG:RPG) {FFFFFF}Dodali ste \"%s\" gume na vozilo.", GetWheelName(modelid));
	}
	if ((response) && (extraid == MODEL_SELECTION_SKINS))
	{

		SCM(playerid, COLOR_GREY, ""SERVERPLAVA"(MG:RPG) "SIVA"Kupio si odecu.");
	  	Update_Skin(playerid, modelid);
	  	PI[playerid][pSkin] = modelid;
	  	g_NovacMinus(playerid, 1000);
    	return 1;
	}
	if ((response) && (extraid == MODEL_SELECTION_VSKINS))
	{

		SCM(playerid, COLOR_GREY, ""SERVERPLAVA"(MG:RPG) "SIVA"Odabrali ste VIP odecu i kosta vas 25 VIP bodova.");
	  	Update_Skin(playerid, modelid);
	  	PI[playerid][pSkin] = modelid;
	    PI[playerid][pVIPBodovi] -= 25 ;
    	return 1;
	}
	return 1;
}

public OnPlayerTakeDamage(playerid, issuerid, Float:amount, weaponid, bodypart)
{
    if(issuerid == INVALID_PLAYER_ID && IsPlayerInWater(playerid) && RonilacOprema[playerid] == 1)
	{
		SetPlayerHealth_H(playerid, 99.0);
	}
	if(bodypart == 9 && AdminDuty[playerid] == 0) // headshot
	{
 		if(IsPlayerConnected(issuerid) && playerid != issuerid && weaponid >= 22 && weaponid <= 34)
		{
	  	  	SetPlayerHealth_H(playerid, 0);
 		}
	}
	if(bodypart >= 3 && bodypart == 3 && AdminDuty[playerid] == 0)
	{
	    if(weaponid == 34) SetPlayerHealth_H(playerid, 0);	// snajper
	    if(weaponid == 31) AddHp(playerid, -12, 100);    	// m4
	    if(weaponid == 30) AddHp(playerid, -10, 100);    	// ak47
		if(weaponid == 22) AddHp(playerid, -4, 100); 		// colt
		if(weaponid == 23) AddHp(playerid, -4, 100);     	// colt SD
		if(weaponid == 24) AddHp(playerid, -15, 100);    	// deagle
		if(weaponid == 25) AddHp(playerid, -18, 100);    	// shotgun
		if(weaponid == 26) AddHp(playerid, -14, 100);   	// swedoff
		if(weaponid == 26) AddHp(playerid, -16, 100);    	// combat shtg
		if(weaponid == 28) AddHp(playerid, -5, 100);     	// uzi
		if(weaponid == 29) AddHp(playerid, -7, 100);     	// smg
		if(weaponid == 32) AddHp(playerid, -6, 100);    	// tec9
		if(weaponid == 33) AddHp(playerid, -13, 100);    	// rifle
	}
	return 1;
}
