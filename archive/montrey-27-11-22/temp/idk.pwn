public OnGameModeExit()
{
 	foreach(Player, i)
	{
		KillTimer_H(freeze[i]);
		KillTimer_H(utovar[i]);
		KillTimer_H(KapijeTimer[i]);
		KillTimer_H(dostavaistovar[i]);
		SavePlayer(i);
	}
	KillTimer_H(adtimer);
    KillTimer_H(wlprovera);
	KillTimer_H(odbrojavanje);
	KillTimer_H(PayDayHad);
	SacuvajDrogaKucu();
	SaveBerza();
	if(pljackabanke == 1) KillTimer_H(robbanktims);
	if(PokrenutEvent == 1) KillTimer_H(eventtajm);
 	if(PokrenutEvent == 2) KillTimer_H(baseballtimers);
	KillTimer_H(antiafk);
    KillTimer_H(proveravrata);
	KillTimer_H(spremikorisnike);
	KillTimer_H(proveravam);
	return 1;
}

