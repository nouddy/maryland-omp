YCMD:rent(playerid,params[], help)
{
    #pragma unused help
    #pragma unused params
    if(!IsPlayerInAnyVehicle(playerid)) return Error(playerid, "Morate biti u rent vozilu!");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return Error(playerid, "Morate biti na mestu vozaca!");
	if(Rentano[playerid] != -1) { Error(playerid,"Vec imas rentano vozilo!"); RemovePlayerFromVehicle(playerid); TogglePlayerControllable(playerid, 1); return 1; }
    foreach(Player, i)
	{
        if(Rentano[i] == GetPlayerVehicleID(playerid))
        {
     		SCM(playerid,GRAD2, " To vozilo je vec rentano!");
     		RemovePlayerFromVehicle(playerid);
			TogglePlayerControllable(playerid, 1);
     		return true;
       	}
    }
    if(BiciklaRent(GetPlayerVehicleID(playerid)))
	{
		if(PI[playerid][pMoney] < 30) { Error(playerid, "Nemate dovoljno novca, potrebno vam je 30$"); RemovePlayerFromVehicle(playerid); TogglePlayerControllable(playerid, 1); return 1; }
		SCM(playerid,GRAD2," Rentali ste biciklo za 30$.");
		g_NovacMinus(playerid, 30);
		//FirmaNovac(93, 30);
	}
	if(MotoRent(GetPlayerVehicleID(playerid)))
	{
		if(PI[playerid][pMoney] < 80) { Error(playerid, "Nemate dovoljno novca, potrebno vam je 80$"); RemovePlayerFromVehicle(playerid); TogglePlayerControllable(playerid, 1); return 1; }
		SCM(playerid,GRAD2," Rentali ste motor za 80$.");
		g_NovacMinus(playerid,80);
		//FirmaNovac(92, 80);
	}
	if(AutaRent(GetPlayerVehicleID(playerid)))
	{
		if(PI[playerid][pMoney] < 120) { Error(playerid, "Nemate dovoljno novca, potrebno vam je  120$"); RemovePlayerFromVehicle(playerid); TogglePlayerControllable(playerid, 1); return 1; }
		SCM(playerid,GRAD2," Rentali ste auta za 120$.");
		g_NovacMinus(playerid,120);
		//FirmaNovac(91, 120);
	}
	Rentano[playerid] = GetPlayerVehicleID(playerid);
	TogglePlayerControllable(playerid, 1);
	return true;
}
