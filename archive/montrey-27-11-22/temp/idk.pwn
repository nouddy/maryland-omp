YCMD:usedrugs(playerid, params[],help)
{
	new vrsta, Float:Helti, string[128];
	if(sscanf(params, "i", vrsta))
	{
	    Koristi(playerid, "usedrugs [Droga(1-3)]" );
	    SCM(playerid, GRAD2, "2. Kokain  3. Cannabis");
	    return 1;
	}
	if(vrsta == 2)
	{
	    if(KoristiDrogu[playerid] == true) return Error(playerid, "Vec koristite drogu.");
	    GetPlayerHealth(playerid, Helti);
	    if(PI[playerid][Kokain] == 0) return Error(playerid, "Nemate kokaina!");
	    //if(Helti >= 100.0) return Error(playerid, "Ne mozes koristiti kokain jer mozes umrijeti od predoziranja." );
		SetPlayerHealth_H(playerid, Helti+25.0);
		SetPlayerWeather(playerid, -66);
		SetPlayerDrunkLevel(playerid, 13000);
		PI[playerid][Kokain] -= 1;
		KoristiDrogu[playerid] = true;
		format(string, sizeof(string), "* %s vadi kokain iz dzepa i smrce ga kroz nos.",Ime_Igraca(playerid));
		ProxDetector(20.0, playerid, string,PROX,PROX,PROX,PROX,PROX);
		if(Helti >= 99.0)
		{
		    SetPlayerHealth_H(playerid, 0);
		    SCM_SGC(playerid, "Kurac rodjo predoziro si se");
		}
		else
		{
		    SetTimerEx("DrogaTimer", 30000, 0, "i", playerid);
		}
	}
	else if(vrsta == 3)
	{
	    if(KoristiDrogu[playerid] == true) return Error(playerid, "Vec koristite drogu.");
	    GetPlayerHealth(playerid, Helti);
	    if(PI[playerid][Cannabis] == 0) return Error(playerid, "Nemate Cannabis-a!");
	    //if(Helti <= 50.0) return Error(playerid, "Ne mozes koristiti Cannabis jer mozes umrijeti od predoziranja." );
		SetPlayerHealth_H(playerid, Helti+25.0);
		SetPlayerWeather(playerid, -66);
		SetPlayerDrunkLevel(playerid, 13000);
		PI[playerid][Cannabis] -= 1;
		KoristiDrogu[playerid] = true;
		pero[playerid] = SetTimerEx("RegenerirajHelth", 10000, true, "i", playerid);
		format(string, sizeof(string), "* %s vadi spricu sa Cannabis-om i bode se u venu.",Ime_Igraca(playerid));
		ProxDetector(20.0, playerid, string,PROX,PROX,PROX,PROX,PROX);
		if(Helti >= 99.0)
		{
		    SetPlayerHealth_H(playerid, 0);
		    SCM_SGC(playerid, "Kurac rodjo predoziro si se");
		}
		else
		{
		    SetTimerEx("DrogaTimer", 45000, 0, "i", playerid);
		}
	}
	return 1;
}
