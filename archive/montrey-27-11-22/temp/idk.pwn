YCMD:zavezi(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	new id, string[128];
	if(PI[playerid][pKanap] <= 0) return Error(playerid, "Nemate kanap.");
	if(gettime() < KanapVreme[playerid]) return Error(playerid, "Ovu komandu mozete koristiti svakih 10000 sekundi.");
	if(sscanf(params, "u", id)) return Koristi(playerid, "zavezi [ID/Dio Imena]");
	if(!IsPlayerConnected(id)) return Error(playerid, "Pogresan ID.");
	if(PI[playerid][Zatvor] != 0) return Error(playerid, "Ne mozes vezati igraca koji je u zatvoru.");
    if(!DaljinaIgraca(5.0,playerid,id)) return Error(playerid, "Taj igrac nije blizu tebe.");
	if(GetPlayerVehicleID(id) != GetPlayerVehicleID(playerid)) return Error(playerid, "Igrac nije sa vama u vozilu.");
	if(IgracTazovan[id] == 1) return Error(playerid, "Igrac je tazovan ne mozes ga vezati.");
	if(StavljeneLisice[id] == 1) return Error(playerid, "Igrac ima lisice ne mozes ga vezati.");
   	if(IgracTazovan[playerid] == 1) return Error(playerid, "Tazovan si ne mozes nikoga vezati.");
	if(StavljeneLisice[playerid] == 1) return Error(playerid, "Imas lisice ne mozes nikoga vezati.");
    if(GetPlayerState(id) == PLAYER_STATE_DRIVER) return Error(playerid, "Ne mozete zavezati vozaca.");
	if(Zavezan[id] == 1) return Error(playerid, "Igrac je vec zavezan.");
	if(Zavezan[playerid] == 1) return Error(playerid, "Vi ste zavezani ne mozete nikoga vezati.");
	Zavezan[id] = 1;
	TogglePlayerControllable(id, 0);
	GameTextForPlayer(id, "~r~Zavezani ste!", 5, 5000);
	format(string,sizeof(string),"* Zavezali ste igraca %s ",Ime_Igraca(id));
	SCM(playerid,ZUTA,string);
	format(string,sizeof(string),"* Svezan si od strane %s ",Ime_Igraca(playerid));
	SCM(id,ZUTA,string);
	format(string,sizeof(string), "%s je zavezao kanapom %s, da nebi pobegao.", Ime_Igraca(playerid),Ime_Igraca(id));
	ProxDetector(20.0, playerid, string, PROX,PROX,PROX,PROX,PROX);
	KanapVreme[playerid] = gettime() + 10000;
	return 1;
}
YCMD:odvezi(playerid, params[],help)
