	if(IsPlayerInRangeOfPoint(playerid, 5.0,0,0,0)||d==-1)return Error(playerid, "Ne nalazite se u blizini auto salona!");
	if(SalonInfo[d][Gledan] == 1) return Error(playerid, "Neko vec razgleda vozila ovog auto salona!");
	if(SalonInfo[d][TipCosa] == 0) return Error(playerid, "Auto salon vise nema vozila u ponudi!");
