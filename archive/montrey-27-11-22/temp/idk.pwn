	else if(PI[playerid][Clan] == 5) strmid(SurenosInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
	else if(PI[playerid][Clan] == 6) strmid(VagosInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
	else if(PI[playerid][Clan] == 7) strmid(LCNInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
	else if(PI[playerid][Clan] == 8) strmid(YakuzaInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
