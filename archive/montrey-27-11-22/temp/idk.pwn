YCMD:koristi(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
    if(PI[playerid][Zatvor] > 0) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Ne mozete koristiti ovu komandu jer ste u zatvoru.");
	if(IsPlayerInAnyVehicle(playerid)) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Nemozes koristiti tu komandu kad si u vozilu.");
	new koristi[32];
	if(sscanf(params, "s[32]", koristi)) return Koristi(playerid,"koristi [hranu]");
	else if(!strcmp(koristi, "hranu"))
	{
	    if(PI[playerid][pHrana] <= 0) return SCM(playerid,-1,""C_BELA"Nemate hrane.");
	    new Float:hp, Float:nhp; GetPlayerHealth(playerid, hp);
		nhp = floatadd(hp, 49.5); SetPlayerHealth_H(playerid, nhp);
		PI[playerid][pHrana] --;
		SCM(playerid,-1,"Pojeli ste hranu koju ste kupili!");
		if(nhp > 99.0) SetPlayerHealth_H(playerid, 99.0);
	}
	else return SCM(playerid,SERVERBOJA,"(MG:RPG) "C_BELA"Nepoznato ime.");
	return 1;
}
