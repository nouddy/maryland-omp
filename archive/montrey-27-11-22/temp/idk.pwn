			}
			else if(PI[playerid][Admin] >= 1 && AdminDuty[playerid] == 1)
			{
			    format(string, sizeof(string), "{000000}%s: {FFFFFF}%s", Ime_Igraca(playerid), text);
				ProxDetector(15.0, playerid, string,GRAD2,GRAD2,GRAD2,GRAD2,GRAD2);
			}
		}
  		else
  		{
  		    format(string, sizeof(string), "{E6E6E6}%s: {FFFFFF}%s", Ime_Igraca(playerid), text);
			ProxDetector(15.0, playerid, string, GRAD2,GRAD2,GRAD2,GRAD2,GRAD2);
		}
		return 0;
	}
	return 1;
