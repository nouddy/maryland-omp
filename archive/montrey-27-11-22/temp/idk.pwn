		{
			if(!IsPlayerInAnyVehicle(playerid) && !IsPlayerInAnyVehicle(IDVucenogIgraca[playerid])
			|| !IsPlayerInAnyVehicle(playerid) && IsPlayerInAnyVehicle(IDVucenogIgraca[playerid]))
			{
				new Float:plx, Float:ply, Float:plz;
				GetPlayerPos(playerid, plx, ply, plz);
				SetPlayerPos(IDVucenogIgraca[playerid], plx,  ply+0.4, plz);
