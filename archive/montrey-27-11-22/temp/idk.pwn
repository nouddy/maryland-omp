	if(sscanf(params,"i",id)) return Koristi(playerid,"gotofirma [ID]");
	format(viFile, sizeof(viFile), FIRMA_FILE, id);
	if(!fexist(viFile)) return Error(playerid, "ID te firme ne postoji.");
    SetPlayerPos(playerid,FirmaInfo[id][fUlazX], FirmaInfo[id][fUlazY], FirmaInfo[id][fUlazZ]);
    g_SetPlayerInterior(playerid,0); g_SetPlayerVirtualWorld(playerid,0);
	return 1;
}
YCMD:efirma(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if(PI[playerid][Admin] < 5) return Ovlascenje(playerid);
	new i = -1;
	for(new x; x < MAX_FIRMI; x++)
	{
	    if(IsPlayerInRangeOfPoint(playerid, 3.0, FirmaInfo[x][fUlazX], FirmaInfo[x][fUlazY], FirmaInfo[x][fUlazZ]))
		{
			i = x;
			IzmenaFirme[playerid] = x;
			break;
		}
	}
	if(i == -1) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Niste kod firme.");
	new akcija[16];
	if(sscanf(params, "s[16]", akcija))
    {
        Koristi(playerid, "efirma [prodaj, info, otkljucaj, zakljuca, edit]");
		return 1;
	}
	if(!strcmp(akcija, "prodaj", true))
	{
		if(FirmaInfo[i][fImaVlasnika] == 0) return Error(playerid, "Ne mozete prodati firmu jer ta firma nema vlasnika.");
		FirmaInfo[i][fImaVlasnika] = 0;
		FirmaInfo[i][fKasa] = 0;
		FirmaInfo[i][fZatvoreno] = 1;
		strmid(FirmaInfo[i][fVlasnik], "Niko", 0, strlen("Niko"), 255);
		strmid(FirmaInfo[i][fIznuda], "Niko", 0, strlen("Niko"), 255);
		PI[playerid][pPosedovanjeFirme] = -1;
		FirmaLabelIPickup(i);
		SacuvajFirmu(i);
		SCMF(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Prodali ste firmu ID: %d!", i);
		IzmenaFirme[playerid] = -1;
	}
	else if(!strcmp(akcija, "info", true))
	{
		new z[3];
	   	if(FirmaInfo[i][fZatvoreno] == 1) z = "Da";
	    else z = "Ne";
	    SCMF(playerid,-1,"- Vlasnik: %s | Reket: %s | Naziv: %s | Nivo: %d | Zakljucano: %s", FirmaInfo[i][fVlasnik], FirmaInfo[i][fIznuda],FirmaInfo[i][fNaziv],FirmaInfo[i][fNivo],z);
 	    SCMF(playerid,-1,"- Cena: %d$ | ID: %d | Kasa: %d$ | Proizvodi: %d/%d | Neaktivnost: %d h", FirmaInfo[i][fCena], i, FirmaInfo[i][fKasa], FirmaInfo[i][fProizvodi], FirmaInfo[i][fMaxProizvodi], FirmaInfo[i][fNeaktivnost]);
		IzmenaFirme[playerid] = -1;
	}
	else if(!strcmp(akcija, "otkljucaj", true))
	{
		FirmaInfo[i][fZatvoreno] = 0;
		SCM(playerid,-1,""C_BELA"Firma je otkljucana.");
		SacuvajFirmu(i);
		IzmenaFirme[playerid] = -1;
	}
	else if(!strcmp(akcija, "zakljucaj", true))
	{
		FirmaInfo[i][fZatvoreno] = 1;
		SCM(playerid,-1,""C_BELA"Firma je zakljucana.");
		SacuvajFirmu(i);
		IzmenaFirme[playerid] = -1;
	}
	else if(!strcmp(akcija, "edit", true))
	{
		SPD(playerid, 7799, DIALOG_STYLE_LIST, "{FFFFFF}Edit firme", "Izmeni naziv\nIzmeni nivo\nIzmeni cenu\nIzmeni proizvode\nIzmeni kasa novac\nIzmeni reket", "Odaberi", "Izadji");
	}
	else return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Netacno uneseno radnja editovanja.");
	return 1;
}
YCMD:kupifirmu(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	for(new i; i < sizeof(FirmaInfo); i++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 3.0, FirmaInfo[i][fUlazX], FirmaInfo[i][fUlazY], FirmaInfo[i][fUlazZ]))
		{
			if(!strcmp(FirmaInfo[i][fVlasnik], "Niko", true) && FirmaInfo[i][fImaVlasnika] == 0)
			{
				if(PI[playerid][pNivo] < FirmaInfo[i][fNivo]) return SCMF(playerid,-1, ""SERVERPLAVA"(MG:RPG) "C_BELA"Treba vam %d nivo za ovu firmu!", FirmaInfo[i][fNivo]);
				if(PI[playerid][pPosedovanjeFirme] != -1) return Error(playerid, "Vec imas firmu.");
				if(PI[playerid][pMoney] < FirmaInfo[i][fCena]) return Error(playerid, "Nemas dovoljno novca za ovo firmu.");
				strmid(FirmaInfo[i][fVlasnik], Ime_Igraca(playerid), 0, strlen(Ime_Igraca(playerid)), 255);
				FirmaInfo[i][fImaVlasnika] = 1;
				PI[playerid][pPosedovanjeFirme] = i;
				FirmaInfo[i][fZatvoreno] = 0;
				SCM(playerid,-1, ""SERVERPLAVA"(MG:RPG) "C_BELA"Cestitamo, kupili ste firmu.");
				SCM(playerid,-1, ""SERVERPLAVA"(MG:RPG) "C_BELA"Za komande firme koristite /firma.");
				g_NovacMinus(playerid, FirmaInfo[i][fCena]);
				strmid(FirmaInfo[i][fIznuda],"Niko",0,strlen("Niko"),255);
				UsaoFirma[playerid] = i;
				FirmaLabelIPickup(i);
				SacuvajFirmu(i);
				return 1;
			}
			else return Error(playerid, "Firma nije na prodaju.");
		}
	}
	return 1;
}
