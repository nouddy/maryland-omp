	PlayerTextDrawShow(playerid, COS_TD[playerid][1]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][2]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][3]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][4]);
	PlayerTextDrawSetPreviewModel(playerid, COS_TD[playerid][5], ListaInfo[playerid][lista][Model]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][5]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][6]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][7]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][8]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][9]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][10]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][11]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][13]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][14]);
	PlayerTextDrawShow(playerid, COS_TD[playerid][15]);
	ShowEditingTextdraw(playerid, 1);
	SelectTextDraw(playerid, BELA);
	SalonInfo[d][Svehicleid] = AddStaticVehicle(ListaInfo[playerid][1][Model], SalonInfo[d][VoPPozX],SalonInfo[d][VoPPozY],SalonInfo[d][VoPPozZ],SalonInfo[d][VoPAngel],-1,-1);
	SetPlayerCameraPos(playerid,SalonInfo[d][Lokx],SalonInfo[d][Loky],SalonInfo[d][Lokz]);
	SetPlayerCameraLookAt(playerid,SalonInfo[d][Lokatx],SalonInfo[d][Lokaty],SalonInfo[d][Lokatz]);
