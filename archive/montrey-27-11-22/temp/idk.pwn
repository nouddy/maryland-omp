YCMD:sirena(playerid, params[],help)
{
	if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if(!IgracPolicajac(playerid)) return Error(playerid, "Niste clan drzavnih bezbednosti.");
	if(OfficerNaDuznosti[playerid] == 0) return Error(playerid, "Niste na duznost.");
	new vehicleid = GetPlayerVehicleID(playerid);

	if (!IsPlayerInAnyVehicle(playerid))
	    return Error(playerid, "Moras biti u vozilu.");

    new type;
	if(sscanf(params, "d", type)) return Koristi(playerid, "sirena (0 stavi | 1 makni)");
	switch (type)
	{
	    case 0:
	    {
	        if(Siren[vehicleid] == 1) return Error(playerid, "Na vozilu vec ima sirena.");

			static
        		Float:fSize[3],
        		Float:fSeat[3];

		    GetVehicleModelInfo(GetVehicleModel(vehicleid), VEHICLE_MODEL_INFO_SIZE, fSize[0], fSize[1], fSize[2]); // need height (z)
    		GetVehicleModelInfo(GetVehicleModel(vehicleid), VEHICLE_MODEL_INFO_FRONTSEAT, fSeat[0], fSeat[1], fSeat[2]); // need pos (x, y)

            Siren[vehicleid] = 1;
			SirenObject[vehicleid] = CreateDynamicObject(18646, 0.0, 0.0, 1000.0, 0.0, 0.0, 0.0);

		    AttachDynamicObjectToVehicle(SirenObject[vehicleid], vehicleid, -fSeat[0], fSeat[1], fSize[2] / 2.0, 0.0, 0.0, 0.0);
			format(nellys, sizeof(nellys), "** %s stavlja sirenu na vozilo.", Ime_Igraca(playerid));
			ProxDetector(10.0, playerid, nellys, PROX,PROX,PROX,PROX,PROX);
   		}
		case 1:
		{
		    if(Siren[vehicleid] == 0) return Error(playerid, "Na vozilu nema sirena.");
		    
		    Siren[vehicleid] = 0;
			DestroyDynamicObject(SirenObject[vehicleid]);
			format(nellys, sizeof(nellys), "** %s skida sirenu sa vozila.", Ime_Igraca(playerid));
			ProxDetector(10.0, playerid, nellys, PROX,PROX,PROX,PROX,PROX);
		}
		default:
     	{
			Koristi(playerid, "sirena (0 stavi | 1 makni)");
   		}
	}
	return 1;
}
YCMD:pban(playerid, params[],help)
{
	if(PI[playerid][Admin] >= 2 || IsPlayerAdmin(playerid))
	{
		new id, rsn[128],time;
		if(sscanf(params, "uis[128]", id, time, rsn)) return Koristi(playerid, "pban [ID/Dio Imena] [Dani 1-26] [Razlog]");
		if(time > 26 || time < 1) return Error(playerid, "Dani ne mogu ici preko 26 i ispod 1!");
		if(!IsPlayerConnected(id))return Error(playerid, "On/ona nije konektovan.");
		new d,m,y,bx,ca;
	    getdate(y,m,d);
		bx = d+time;
		if(m == 12 && bx > 31) { ca = bx-31; m = 1; y++; }
		else if(bx > 31 && m == 1) { ca = bx-31; m++; }
		else if(m == 2 && bx > 29 && y == 2015) { ca = bx-29; m++; }
		else if(m == 2 && bx > 28 && y == 2016) { ca = bx-28; m++; }
		else if(bx > 31 && m == 3) { ca = bx-31; m++; }
		else if(bx > 30 && m == 4) { ca = bx-30; m++; }
		else if(bx > 31 && m == 5) { ca = bx-31; m++; }
		else if(bx > 30 && m == 6) { ca = bx-30; m++; }
		else if(bx > 31 && m == 7) { ca = bx-31; m++; }
		else if(bx > 31 && m == 8) { ca = bx-31; m++; }
		else if(bx > 30 && m == 9) { ca = bx-30; m++; }
		else if(bx > 31 && m == 10) { ca = bx-31; m++; }
		else if(bx > 30 && m == 11) { ca = bx-30; m++; }
		else if(bx > 31 && m == 12) { ca = bx-31; m++; }
		else ca = bx;
		PBanInfo[id][pBanDani] = ca;
		PBanInfo[id][pBanMesec] = m;
		PBanInfo[id][pBanGodina] = y;
		format(nellys, sizeof(nellys), "PBAN |{FFFFFF} Admin {FF0000}%s {FFFFFF}je privremeno banovao igraca {FF0000}%s {FFFFFF}|| Razlog: {FF0000}%s {FFFFFF}|| Datum unbana:{FF0000} %d/%d/%d", Ime_Igraca(playerid), Ime_Igraca(id), rsn, PBanInfo[id][pBanDani], PBanInfo[id][pBanMesec], PBanInfo[id][pBanGodina]);
        StaffPoruka(COLOR_YELLOW, nellys);
        format(nellys,sizeof(nellys),"{F23A0D}[BAN]: {FFFFFF}%s je banovan od strane admina, razlog: {FFFFFF}%s",Ime_Igraca(id),rsn);
		SCMTA(GRAD2,nellys);
		SacuvajPBan(id);
		PI[id][BanovanPuta]++;
		new string[256];
		format(string,sizeof(string), "Privremeni ban | {F81414}Banovan si od administratora {FFFFFF} %s{F81414} | Razlog{FFFFFF} %s  ",Ime_Igraca(playerid), rsn);
		new str2[256];
		format(str2, sizeof(str2), "{00CED1}Datum unbana: %d.%d.%d.", PBanInfo[id][pBanDani], PBanInfo[id][pBanMesec], PBanInfo[id][pBanGodina]);
		SCM(id, BELA, "======= BAN INFO =======");
		SCM(id, BELA, string);
		SCM(id, BELA, str2);
		SCM(id, BELA, "========================");
		SetTimerEx("KickTajmer", 50, false, "i", id);
		format(logstring, sizeof logstring, "| Igrac: %s | Admin: %s | Razlog: %s", Ime_Igraca(playerid),Ime_Igraca(id), rsn);
		UpisiLog(FILE_PBAN, logstring);
		return 1;
	}
	else Ovlascenje(playerid);
	return 1;
}
