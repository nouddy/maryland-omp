// ----------------------- OFFLINE LOADING DATA --------------------------------
stock NV_Int(filename[],key[])
{
   return strval(NV_Get(filename,key));
}

stock NV_Get(filename[],key[])
{
	new tmpres[CHECK_MAX_STRING];

	new key_length = strlen(key);
	if (key_length==0 || key_length + 2 > CHECK_MAX_STRING) return tmpres;

	new File:fohnd;
	fohnd=fopen(filename,io_read);
	if (!fohnd) return tmpres;

	while (fread(fohnd,tmpres))
	{
		if(tmpres[key_length]=='=' && !strcmp(tmpres, key, true, key_length))
		{
			NV_StripNewLine(tmpres);
			strmid(tmpres, tmpres, key_length + 1, strlen(tmpres), CHECK_MAX_STRING);
			fclose(fohnd);
			return tmpres;
		}
	}
	fclose(fohnd);
	return tmpres;
}

stock NV_StripNewLine(string[])
{
	new len = strlen(string);
	if (string[0]==0) return ;
	if ((string[len - 1] == '\n') || (string[len - 1] == '\r'))
	{
		string[len - 1] = 0;
		if (string[0]==0) return ;
		if ((string[len - 2] == '\n') || (string[len - 2] == '\r')) string[len - 2] = 0;
	}
}
