	if(GetPlayerVehicleID(id) != GetPlayerVehicleID(playerid)) return Error(playerid, "Igrac se ne nalazi u vasem vozilu.");
	RemovePlayerFromVehicle(id);
	SCMF(playerid, BELA, ""C_ZUTA"* "C_BELA"Izbacili ste %s iz vozila.", Ime_Igraca(id));
	SCMF(id, BELA, ""C_ZUTA"* "C_BELA"%s vas je izbacio iz vozila.", Ime_Igraca(playerid));
	return 1;
}
