forward DodajNeaktivnostVozila();
public DodajNeaktivnostVozila()
{
	for(new vids = 0; vids < sizeof(VoziloInfo); vids++)
    {
        if(VoziloInfo[vids][vImaVlasnika] == 1)
		{
			if(!IsPlayerConnected(GetPlayerID(VoziloInfo[vids][vVlasnik])))
			{
				VoziloInfo[vids][vNeaktivnost] += 1;
				SacuvajVozilo(vids);
			}
			else
			{
				VoziloInfo[vids][vNeaktivnost] = 0;
				SacuvajVozilo(vids);
			}
			if(VoziloInfo[vids][vNeaktivnost] > 500)
			{
			    for(new p = 0; p < sizeof(PozicijePijacaZauzeta); p++)
				{
					if(PozicijePijacaZauzeta[p] == INVALID_VEHICLE_ID)
					{
					    DestroyVehicle(VoziloInfo[vids][vID]);
