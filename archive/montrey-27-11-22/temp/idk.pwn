		format(korisnik, sizeof(korisnik), KORISNICI, account);
		if(!fexist(korisnik)) return Error(playerid, "Korisnicki racun pod tim imenom ne postoji  (nije registrovan)!" );
		if(strfind(korisnik, "Pikoli", true) != -1) return Error(playerid, "Ne mozete obrisati akaunt vlasnika servera!");
		if(strfind(korisnik, "Poponjec", true) != -1) return Error(playerid, "Ne mozete obrisati akaunt vlasnika servera!");
		if(strfind(korisnik, "Jovana", true) != -1) return Error(playerid, "Ne mozete obrisati akaunt vlasnika servera!");
		if(strfind(korisnik, "Djani", true) != -1) return Error(playerid, "Ne mozete obrisati akaunt vlasnika servera!");
		if(IsPlayerConnected(IdByName(account))) return Error(playerid, "Taj igrac je online!");
		if(NV_Int(korisnik, "Admin ") > 0)
		{
			strmid(AdminInfo[NV_Int(korisnik, "AdminID ")][aAdmin], "Niko", 0, strlen("Niko"), 255);
		 	AdminInfo[NV_Int(korisnik, "AdminID ")][aNeaktivnost] = 0;
		 	AdminInfo[NV_Int(korisnik, "AdminID ")][aRank] = 0;
			SacuvajAdmine();
   		}
   		if(NV_Int(korisnik, "Helper ") > 0)
		{
			strmid(HelperInfo[NV_Int(korisnik, "HelperID ")][hHelper], "Niko", 0, strlen("Niko"), 255);
		 	HelperInfo[NV_Int(korisnik, "HelperID ")][hNeaktivnost] = 0;
		 	HelperInfo[NV_Int(korisnik, "HelperID ")][hRank] = 0;
			SacuvajHelpere();
   		}
		SCMF(playerid, PLAVA, "(nalog) {FFFFFF}Uspesno ste Obrisali ste nalog pod imenom {1275ED}%s", account);
		format(nellys, sizeof(nellys), "![WARNING]: Admin %s je obrisao korisnicki racun pod imenom %s.", Ime_Igraca(playerid),account);
