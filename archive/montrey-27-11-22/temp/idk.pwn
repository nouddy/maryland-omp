	new vozilo = GetPlayerVehicleID(playerid);
	if(playerid > BROJ_SLOTOVA) return 1;
	if((newstate == 2 && oldstate == 3) || (newstate == 3 && oldstate == 2)) return Kick(playerid);
    if(!VoziloJeBicikla(GetVehicleModel(GetPlayerVehicleID(playerid))))
	{
		new vIDs = GetPlayerVehicleID(playerid);
