function OxygenBar()
{
    foreach(Player, playerid)
    {
        if(IsPlayerInWater(playerid) && RonilacOprema[playerid] == 1)
		{
		    if(GetPlayerProgressBarValue(playerid, oxygen[playerid]) == 0)
		    {
                SCM(playerid, CRVENA, "Oxygen | Ostali ste bez kiseonika, umrli ste!");
                SetPlayerHealth_H(playerid, 0);
			}
			else if(GetPlayerProgressBarValue(playerid, oxygen[playerid]) > 0)
			{
        		SetPlayerProgressBarValue(playerid, oxygen[playerid], GetPlayerProgressBarValue(playerid, oxygen[playerid])-2);
        		UpdatePlayerProgressBar(playerid, oxygen[playerid]);
         	}
        }
    }
}

