YCMD:clanovi(playerid, params[],help)
{
	new teamnumber;
	new clstring[128];
	if (PI[playerid][Lider] != 0) teamnumber = PI[playerid][Lider];
	else if (PI[playerid][Clan] != 0) teamnumber = PI[playerid][Clan];
	else {
		Error(playerid, "Nisi clan organizacije!");
		return 1;
	}
	if(PI[playerid][Clan] == 1 && PI[playerid][Rank] < 1) return Error(playerid, "Suspendirani ste!");
	SCM(playerid, 0x808000AA, "**Clanovi na serveru**");
	foreach(Player, i)
	{
		if (IsPlayerConnected(i)) {
			format(clstring, sizeof(clstring), "");
			if (PI[i][Lider] == teamnumber) format(clstring, sizeof(clstring), "%s, %s (LIDER)",Ime_Igraca(i), RankOrg(i));
			else if (PI[i][Clan] == teamnumber) format(clstring, sizeof(clstring), "%s, %s (%d).",Ime_Igraca(i), RankOrg(i), PI[i][Rank]);
			if (strlen(clstring) > 1) SCM(playerid, 0xF5DEB3AA, clstring);
			clstring[0] = '\0';
		}
	}
	return 1;
}
