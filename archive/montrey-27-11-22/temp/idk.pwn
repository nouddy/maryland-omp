stock ShowEditingTextdraw(playerid,list)
{
	if(list == -1)
	{
	    for(new i = 0; i < 16; i++)
	 	{
	 	    PlayerTextDrawHide(playerid, COS_TD[playerid][i]);
			PlayerTextDrawDestroy(playerid, COS_TD[playerid][i]);
	 	}
		return 1;
	}
	else
	{
		new string[256], s;
		s = ListaInfo[playerid][list][Model];
		PlayerTextDrawHide(playerid, COS_TD[playerid][8]);
		PlayerTextDrawHide(playerid, COS_TD[playerid][10]);
		PlayerTextDrawHide(playerid, COS_TD[playerid][11]);
		PlayerTextDrawHide(playerid, COS_TD[playerid][12]);
		PlayerTextDrawHide(playerid, COS_TD[playerid][5]);
		////////////////////////////////////////////////
		format(string,sizeof(string),"%s",ImenaVozila[s-400]);
		PlayerTextDrawSetString(playerid, COS_TD[playerid][8], string);
		PlayerTextDrawShow(playerid, COS_TD[playerid][8]);
		////////////////////////////////////////////////
		format(string,sizeof(string),"%d$",ListaInfo[playerid][list][Cena]);
		PlayerTextDrawSetString(playerid, COS_TD[playerid][10], string);
		PlayerTextDrawShow(playerid, COS_TD[playerid][10]);
		////////////////////////////////////////////////
		format(string,sizeof(string),"%d",ListaInfo[playerid][list][Kolicina]);
		PlayerTextDrawSetString(playerid, COS_TD[playerid][12], string);
		PlayerTextDrawShow(playerid, COS_TD[playerid][12]);
		////////////////////////////////////////////////
		PlayerTextDrawSetPreviewModel(playerid, COS_TD[playerid][5], ListaInfo[playerid][list][Model]);
		PlayerTextDrawShow(playerid, COS_TD[playerid][5]);
		////////////////////////////////////////////////
		if(VoziloJeDizelas(s))
		{
	      	PlayerTextDrawSetString(playerid, COS_TD[playerid][11], "Dizel");
		}
		else if(VoziloJeAvion(s))
		{
	 		PlayerTextDrawSetString(playerid, COS_TD[playerid][11], "Kerozin");
		}
		else
  		{
  		    PlayerTextDrawSetString(playerid, COS_TD[playerid][11], "Benzin");
  		}
  		PlayerTextDrawShow(playerid, COS_TD[playerid][11]);
  	}
	return 1;
}
