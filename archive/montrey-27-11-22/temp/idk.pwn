		}
		else if(DrogaInfo[b][drVrsta] == 3)
		{
			ObrisiDrogu(b);
			new grams = randomExx(1,4);
			PI[playerid][Cannabis] += grams;
			SCMF(playerid, SVETLOPLAVA, "** Ubrali ste Cannabis,dobili ste %d grama.", grams);
			ApplyAnimation(playerid, "BOMBER","BOM_Plant_In",4.0,0,0,0,0,0);
		}
	}
	else
 	{
		if(DrogaInfo[b][drVrsta] == 2)
		{
			ObrisiDrogu(b);
			new grams = randomExx(1,2);
			PI[playerid][Kokain] += grams;
			SCMF(playerid, SVETLOPLAVA, "** Ukrali ste tuu sadnicu kokaina,dobili ste %d grama.", grams);
			ApplyAnimation(playerid, "BOMBER","BOM_Plant_In",4.0,0,0,0,0,0);
		}
