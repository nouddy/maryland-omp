YCMD:ukradi(playerid, params[],help)
{
	if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if(PI[playerid][pPosao] != POSAO_DZEPAROS) return Error(playerid, "Niste dzeparos.");
	if(PI[playerid][pDzeparosVreme] != 0) return SCMF(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Morate sacekati jos %d sekundi da bi mogli ponovo krasti!", PI[playerid][pDzeparosVreme]);
	if(PI[playerid][Zatvor] != 0) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Ne mozete krasti pare u zatvoru.");
	new id, Float:Poz[3], ukradeno, rand;
	if(sscanf(params, "u", id)) return Koristi(playerid, "ukradi [ID/Dio Imena]");
	if(!IsPlayerConnected(id)) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac je offline.");
	if(id == playerid) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Ne mozete krasti sami sebi pare.");
	//if(PI[id][pNivo] < 3) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac mora biti nivo 3+ da bi mu krali pare.");
	if(IsPlayerInAnyVehicle(playerid)) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Ne mozete krasti dok ste u vozilu.");
	if(IsPlayerInAnyVehicle(id)) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Ne mozete krasti igraca koji je u vozilu.");

	if(PI[id][pNivo] == 1 && PI[id][pNivo] == 2 && PI[id][pNivo] == 3 && PI[id][pNivo] == 4 && PI[id][pNivo] == 5) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA" Taj igrac je jos Civil idi kradi Burzujima.");

	GetPlayerPos(id, Poz[0], Poz[1], Poz[2]);
	if(!IsPlayerInRangeOfPoint(playerid, 2.0, Poz[0], Poz[1], Poz[2])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Predaleko ste.");
	if(IsPlayerInRangeOfPoint(playerid, 50.0, 2694.1689,-611.0945,-71.6423)) return Error(playerid, "Ne mozete krasti pare u banci.");
    if(IsPlayerInRangeOfPoint(playerid, 50.0, 1461.6193,-1022.1646,23.8281)) return Error(playerid, "Ne mozete krasti pare ispred banke.");
	if(PI[playerid][pMoney] < 400)
	{
	    PostaviWanted(playerid, "Pokusaj kradje novcanika", "Nepoznato");
	    SCM(playerid, ERRORCOLOR, "Niste uspeli da ukradete novac.");
		PI[playerid][pDzeparosVreme] = 200;
	    return 1;
	}
	rand = random(2);
	switch(rand)
	{
		case 0..1:
		{
			ukradeno = PI[id][pMoney] / 100 * 10;
			if(PI[playerid][pMoney] < ukradeno) return PostaviWanted(playerid, "Pokusaj kradje novca", "Nepoznat");
			g_NovacMinus(id, ukradeno); g_NovacPlus(playerid, ukradeno);
			PostaviWanted(playerid, "Kradja novca", "Nepoznat");
			SCMF(playerid, BELA, ""HZELENA"Uspesno ste ukrali pare | Kolicina ukradenog novca "C_BELA"%d$!", ukradeno);
			PI[playerid][pDzeparosVreme] = 400;
			SCMF(id, -1,""ORANGE"Igrac %s vam je ukrao "C_BELA"%d$ "C_CRVENA"novca!",Ime_Igraca(playerid), ukradeno);

		}
		case 2:
		{
	    	PostaviWanted(playerid, "Pokusaj kradje novca", "Nepoznat");
	    	SCM(playerid, BELA, ""ORANGE"Niste uspeli da ukradete novac.");
			PI[playerid][pDzeparosVreme] = 200;
		}
	}
	return 1;
}
