public OnPlayerText(playerid, text[])
{
    if (UlogovanProvera[playerid] != 1)
	{
		Error(playerid, "Morate biti prijavljeni da bi ste mogli pisati.");
		return 0;
	}
	if(strfind(text,"!kill",true) == 0) return Kick(playerid);
    if(strfind(text,"www.samphax.tk",true) == 0) return Kick(playerid);
	if(strlen(text) > 64) { Error(playerid, "Text je predugacak!"); return false; }
    if(PI[playerid][Mutiran] >= 1)
	{
		Error(playerid, "Mutirani ste ne mozete pisati.");
		return 0;
	}
	if(TrakaStavljena[playerid] == true) { Error(playerid, "Imate traku na usta, ne mozete govoriti."); return 0; }
	if(IsAnIP(text))
	{
		format(nellys, sizeof(nellys), "!! [REKLAMA] | %s [ID:%d] | %s", Ime_Igraca(playerid), playerid, text);
		AdminPoruka(0x00FF00FF, nellys);
		SCM(playerid, CRVENA, "Pokusali ste da reklamirate drugi server, prijavljeni ste.");
		return 0;
    }
    if(stringContainsIP(text))
	{
		format(nellys, sizeof(nellys), "!! [IC] | %s [ID:%d] | %s", Ime_Igraca(playerid), playerid, text);
		AdminPoruka(CRVENA, nellys);
    }
