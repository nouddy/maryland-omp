stock GetClosestVehicle(playerid)
{
	new Float:x,Float:y,Float:z,Float:dist,Float:closedist = 9999.0,closeveh;
	for(new i = 0; i < MAX_VOZILA; i++)
	{
		if(GetVehiclePos(i, x, y, z))
		{
			dist = GetPlayerDistanceFromPoint(playerid, x, y, z);
			if(dist < closedist)
			{
				closedist = dist;
				closeveh = i;
			}
		}
	}
	return closeveh;
}
