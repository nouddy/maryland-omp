			SCM(playerid, ZUTA2, nellys);
			format(nellys, sizeof(nellys), "	Njegov VIP paket istice: %d.%d.%d godine", PI[player][pVIPDan],PI[player][pVIPMesec],PI[player][pVIPGodina]);
