				new vw = GetPlayerVirtualWorld(playerid);
				g_SetPlayerVirtualWorld(IDVucenogIgraca[playerid], vw);
			}
			else if(IsPlayerInAnyVehicle(playerid) && !IsPlayerInAnyVehicle(IDVucenogIgraca[playerid]))
			{
				new vehicle = GetPlayerVehicleID(playerid);
				if(IsSeatAvailable(vehicle, 1))
				{
					PutPlayerInVehicle(IDVucenogIgraca[playerid], vehicle, 1);
				}
				else if(IsSeatAvailable(vehicle, 3))
				{
					PutPlayerInVehicle(IDVucenogIgraca[playerid], vehicle, 3);
				}
				else if(IsSeatAvailable(vehicle, 3))
				{
					PutPlayerInVehicle(IDVucenogIgraca[playerid], vehicle, 3);
				}
			}
		}
		else
		{
			IDVucenogIgraca[playerid] = 9999;
			SCM(playerid, -1,""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac kog ste vukli je napustio server.");
