YCMD:pm(playerid, params[], help)
{
	if(PI[playerid][Admin] >= 1 || PI[playerid][GM] >= 1)
 	{
  		new id, poruka[128];
  		new Float:x, Float:y, Float:z;
  		GetPlayerPos(playerid, x, y, z);
		if(sscanf(params, "us[128]", id, poruka)) return Koristi(playerid, "pm [ID/Ime] [tekst]");
		new string[256];
		if(!IsPlayerConnected(id)) return Error(playerid, "Pogresan ID!");
		format(string, sizeof(string), "* PM od %s (%d): %s", Ime_Igraca(playerid), playerid, poruka);
		SendClientMessage(id, 0xE5C43EAA, string);
		PlayerPlaySound(id, 1055, x, y, z);
		format(string, sizeof(string), "* PM za %s (%d): %s", Ime_Igraca(id), id, poruka);
		SendClientMessage(playerid, 0xBBA033AA, string);
    }
    else
    {
        Error(playerid, "Nisi ovlascen!");
        return 1;
    }
	return 1;
}
YCMD:flip(playerid, params[], help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Niste se ulogovali!");
    if(PI[playerid][Admin] < 1) return Error(playerid, "Nisi ovlascen!");
    if(IsPlayerInAnyVehicle(playerid))
  	{
  		new currentveh, Float:angle;
    	currentveh = GetPlayerVehicleID(playerid);
    	GetVehicleZAngle(currentveh, angle);
    	SetVehicleZAngle(currentveh, angle);
    	RepairVehicle(GetPlayerVehicleID(playerid));
    	SetVehicleHealth(GetPlayerVehicleID(playerid), 999);
    	SendClientMessage(playerid, -1, ""SERVERPLAVA"MG:RPG ~ "BIJELA"Okrenuli ste vozilo u normalan polozaj!");
  	}
	else
  	{
    	Error(playerid, "Niste u vozilu!");
    	return 1;
	}
	return 1;
}
YCMD:zauzmi(playerid, params[], help)
{
	new sat, minut, sekund;
	gettime(sat, minut, sekund);
	if(sat >= 1 && sat <= 9) return Error(playerid, "Ne mozete preuzimati teritoriju izmedju 01:00 i 09:00!");
	new gunID = GetPlayerWeapon(playerid);
	if(gunID == 0)
	{
	    Error(playerid, "Morate drzati oruzje u ruci kako bi poceli osvajanje teritorije!");
	    return 1;
	}
	if(!IgracUBandi(playerid)) return Error(playerid, "Niste clan mafije/bande!");
	for(new id = 0; id < sizeof(GZ); id++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 3.0, GZ[id][gPickupx], GZ[id][gPickupy], GZ[id][gPickupz]))
		{
			new string[126];
			if(GZ[id][gGlavna] > 0) return Error(playerid, "Ova teritorija ne moze biti zauzeta!");
			if(GZ[id][gPreuzeto] > 0) return Error(playerid, "Ova teritorija je skoro zauzimana, sacekajte malo!");
            if(GZ[id][gTeam] == PI[playerid][Clan]) return Error(playerid, "Ne mozete zauzimati teritorije svoje mafije/bande!");
			GZ[id][gPreuzeto] = 120;
			SacuvajZonu(id);
			zonaid[playerid] = id;
			zonatime[playerid] = 600;
			zonatimer[playerid] = SetTimerEx("ZonaZauzmi", 1000, true, "i", playerid);
			X_SCM(playerid, "ZAPOCELI STE ZAUZIMANJE TERITORIJE, morate ostati zivi sledecih 10 minuta kako bi je osvojili!");
			X_SCM(playerid, "Ne smete se previse udaljiti od mesta zauzimanja, jer ce zauzimanje biti prekinuto!");

			GangZoneFlashForAll(ZonaID[id], ZonaBoja2(PI[playerid][Clan]));

			if(PI[playerid][Clan] > 0)
			{
				format(string,sizeof(string),""SERVER"[OBAVESTENJE] "C_BELA"Clan vase organizacije pokusava da zauzme tudju teritoriju "SERVER"[ESC MENU-MAP]");
				FamilyPoruka(PI[playerid][Clan],-1,string);
			}
			else
			{
				format(string,sizeof(string),""SERVER"[OBAVESTENJE] "C_BELA"Clan vase organizacije pokusava da zauzme tudju teritoriju "SERVER"[ESC MENU-MAP]");
				FamilyPoruka(PI[playerid][Lider],-1,string);
			}
			return 1;
		}
	}
	return Error(playerid, "Ne nalazite se u blizini tacke za zauzimanje teritorije!");
}
