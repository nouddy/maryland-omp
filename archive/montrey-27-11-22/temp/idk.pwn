forward PlayerGameCrashed(playerid);
public PlayerGameCrashed(playerid)
{
    if(PI[playerid][Crashed] == 1 && PI[playerid][Zatvor] == 0)
	{
	    SetPlayerPos(playerid, PI[playerid][pPos_x], PI[playerid][pPos_y], PI[playerid][pPos_z]);
	    SetPlayerHealth_H(playerid, 99);
	    g_SetPlayerInterior(playerid, PI[playerid][CrashInt]);
	    g_SetPlayerVirtualWorld(playerid, PI[playerid][CrashedVW]);

	    PI[playerid][CrashInt] = 0;
        PI[playerid][CrashInt] = 0;

	    SCM(playerid, 0x04C3E5AA, "Teleportovan si na mestu gde si zadnji put bio. Uzivaj u igri.");
	}
}
function ZonaZauzmi(playerid)
{
	if(!IsPlayerInRangeOfPoint(playerid, 20.0, GZ[zonaid[playerid]][gPickupx], GZ[zonaid[playerid]][gPickupy], GZ[zonaid[playerid]][gPickupz]))
	{
		new id = zonaid[playerid];
		GangZoneStopFlashForAll(ZonaID[id]);
		PlayerTextDrawHide(playerid, klecanjetd[playerid]);
		zonaid[playerid] -= 1;
		zonatime[playerid] -= 1;
		KillTimer_H(zonatimer[playerid]);
		SCM(playerid,BELA,""SERVER"[INFO] "C_BELA"Niste uspeli da zauzmete teritoriju, udaljili ste se od mesta zauzimanja!");
		return 1;
	}
	zonatime[playerid] -= 1;
	new text2[126];
	format(text2, sizeof(text2), "Zauzimanje u toku: %s", KonvertujVreme(zonatime[playerid]));
	PlayerTextDrawSetString(playerid,klecanjetd[playerid], text2);
	PlayerTextDrawShow(playerid, klecanjetd[playerid]);
	if(zonatime[playerid] == 0)
	{
		PlayerTextDrawHide(playerid, klecanjetd[playerid]);
