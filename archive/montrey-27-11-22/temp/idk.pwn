YCMD:prihvati(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	new ime[24];
	if(sscanf(params, "s[24]", ime))
	{
		SCM(playerid,-1,""SERVERPLAVA"(MG:RPG) "C_BELA"/prihvati [Ime]");
		SCM(playerid,-1,""ORANGE"Prihvati Ime: "C_BELA"Pojacanje | Kaznu | Popravku | Lecenje | Taxi");
		return 1;
	}
	if(!strcmp(ime, "pojacanje", true))
	{
		if(PI[playerid][Clan] == 1 || PI[playerid][Lider] == 1) return Error(playerid, "Samo pripadnici policije.");
		if(OfficerNaDuznosti[playerid] == 0) return Error(playerid, "Niste uzeli znacku(duznost) pa nemozete koristi tu komandu.");
		if(PozvaoPojacanje == -1) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Niko nije pozvao pojacanje.");
		new string[128], Float:xxx,Float:yyy, Float: zzz; GetPlayerPos(PozvaoPojacanje, xxx, yyy, zzz);
		PosaoGPS[playerid] = 1; SetPlayerCheckpoint(playerid, xxx, yyy, zzz, 3.0);
		SCM(playerid, BELA, ""C_BELA"Prihvatili ste poziv za pojacanje idite na crveni marker.");
		format(string, sizeof(string), "Policajac %s je prihvatio vas poziv za pojacanje!", Ime_Igraca(playerid));
		SCM(PozvaoPojacanje, SVETLOPLAVA, string);
	}
	else if(!strcmp(ime, "kaznu", true))
 	{
		new Float:Poz[3];
		GetPlayerPos(KaznuNapisao[playerid], Poz[0], Poz[1], Poz[2]);
		if(PI[playerid][SatiIgre] < 2) return Error(playerid,"Morate imati minimum jedan sat igre za koriscenje ove komande.");
		if(KaznuNapisao[playerid] == -1) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Niko vam nije napisao kaznu.");
		if(!IsPlayerInRangeOfPoint(playerid, 5.0, Poz[0], Poz[1], Poz[2])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Predaleko ste od igraca koji vam je napisao kaznu.");
		if(!IsPlayerConnected(KaznuNapisao[playerid])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac koji vam je napisao kaznu je offline.");
		if(PI[playerid][pMoney] < CenaKazne[playerid]) return Error(playerid, "Nemas dovoljno novca da platis kaznu.");
		SCMF(playerid, ZUTA, "* Platili ste kaznu u iznosu od %d$.", CenaKazne[playerid]);
		SCMF(KaznuNapisao[playerid], ZUTA, "* %s je platio kaznu u iznosu od %d$.", Ime_Igraca(playerid), CenaKazne[playerid]);
		g_NovacMinus(playerid, CenaKazne[playerid]);
		g_NovacPlus(KaznuNapisao[playerid], CenaKazne[playerid]);
		CenaKazne[playerid] = 0; KaznuNapisao[playerid] = -1;
	}
	else if(!strcmp(ime, "popravku", true))
	{
	    if(PonudioPopravku[playerid] == -1) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Nije vam niko ponudio popravku.");
	    if(!IsPlayerConnected(PonudioPopravku[playerid])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac koji vam je ponudio popravku je izasao sa servera.");
		new Float:Poz[3];
		if(PI[playerid][SatiIgre] < 2) return Error(playerid,"Morate imati minimum jedan sat igre za koriscenje ove komande.");
		GetPlayerPos(PonudioPopravku[playerid], Poz[0], Poz[1], Poz[2]);
		if(!IsPlayerInRangeOfPoint(playerid, 5.0, Poz[0], Poz[1], Poz[2])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Niste kod igraca koji vam je ponudio popravku.");
		if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Morate biti u vozilu.");
		if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Morate biti na mestu vozaca.");
		if(IsPlayerInAnyVehicle(PonudioPopravku[playerid])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac koji vam je ponudio popravku ne moze biti u vozilu.");
		if(PI[playerid][pMoney] < CenaPopravke[playerid]) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Nemate dovoljno novca za popravku.");
		g_NovacMinus(playerid, CenaPopravke[playerid]);
		g_NovacPlus(PonudioPopravku[playerid], CenaPopravke[playerid]);
		PopraviVozilo(GetPlayerVehicleID(playerid));
		SCMF(playerid, BELA, "(MG:RPG) "HZELENA"Vozilo je uspesno popravljeno za %d$!", CenaPopravke[playerid]);
		SCMF(PonudioPopravku[playerid], BELA, "(MG:RPG) "HZELENA"Igrac %s je prihvatio popravku za %d$.", Ime_Igraca(playerid), CenaPopravke[playerid]);
        /*FirmaNovac(96, 80);*/ PonudioPopravku[playerid] = -1; CenaPopravke[playerid] = 0;
	}
	else if(!strcmp(ime, "lecenje", true))
	{
	    if(PI[playerid][SatiIgre] < 2) return Error(playerid,"Morate imati minimum jedan sat igre za koriscenje ove komande.");
	    if(LecenjeOd[playerid] == -1) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Niko vam nije ponudio lecenje.");
	    if(!IsPlayerConnected(LecenjeOd[playerid])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac koji vam je ponudio lecenje nije na serveru.");
		new Float:Poz[3]; GetPlayerPos(LecenjeOd[playerid], Poz[0], Poz[1], Poz[2]);
		if(!IsPlayerInRangeOfPoint(playerid, 4.0, Poz[0], Poz[1], Poz[2])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Niste u blizini igraca koji vam je ponudio lecenje.");
		if(IsPlayerInAnyVehicle(playerid) && !IsPlayerInAnyVehicle(LecenjeOd[playerid])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Ne mozete biti u vozilu.");
		if(!IsPlayerInAnyVehicle(playerid) && IsPlayerInAnyVehicle(LecenjeOd[playerid])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Taj igrac je u vozilu.");
		if(PI[playerid][pMoney] < CenaLecenja[playerid]) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Nemate dovoljno novca.");
		g_NovacMinus(playerid, CenaLecenja[playerid]); g_NovacPlus(LecenjeOd[playerid], CenaLecenja[playerid]);
		SetPlayerHealth_H(playerid, 99.0);
		SCMF(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Izleceni ste za od bolnicara %s za %d$!", Ime_Igraca(LecenjeOd[playerid]), CenaLecenja[playerid]);
		SCMF(LecenjeOd[playerid], BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac %s je prihvatio lecenje i zaradio je %d$!", Ime_Igraca(playerid), CenaLecenja[playerid]);
		LecenjeOd[playerid] = -1; CenaLecenja[playerid] = 0;
	}
	else if(!strcmp(ime, "taxi", true))
	{
	    if(PI[playerid][pPosao] != POSAO_TAXISTA) return Error(playerid, "Niste taksista.");
	    if(CenaKarte[playerid] < 1) return Error(playerid, "Niste na duznosti!");
        if(GetPlayerVehicleID(playerid) < vtaxi[0] || GetPlayerVehicleID(playerid) > vtaxi[6]) return Error(playerid, "Niste u taxi-u!");
		if(TaxiPozivalac == -1) return Error(playerid, "Niko nije pozvao taxi.");
        hPrihvatio[playerid] = TaxiPozivalac;
        TaxiPozivalac = -1;
        new Float:Poz[3];
        GetPlayerPos(hPrihvatio[playerid], Poz[0], Poz[1], Poz[2]);
        h_SetPlayerCheckpoint(playerid, Poz[0], Poz[1], Poz[2], 10.0);
        SCM(playerid, SVETLOPLAVA, "* Prihvatili ste poziv. Pozicija igraca je oznacena na Vasem radaru.");
        format(nellys, sizeof nellys, "[INFO]: Taxista %s je prihvatio poziv od %s.", Ime_Igraca(playerid), Ime_Igraca(hPrihvatio[playerid]));
        foreach(Player, i)
        {
            if(PI[i][pPosao] == POSAO_TAXISTA) SCM(i, 0xFFEB00FF, nellys);
		}
        SCMF(hPrihvatio[playerid], SVETLOPLAVA, "* Taxista %s je prihvatio vas poziv. Sacekajte.", Ime_Igraca(playerid));
	}
	else return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Tog imena za prihvacanje nema u listi.");
    return 1;
}
