LoadCosa()
{
	for(new i= 0; i < MAX_SALONA;i++)
	{
		new textic[64];
		format(textic, sizeof(textic), SALONI_FILE,i);
		if(fexist(textic))
		{
			INI_ParseFile(textic, "LoadSalon_%s", .bExtra = true, .extra = i);
			PostaviCosLabela(i);
		}
	}
	return 1;
}
