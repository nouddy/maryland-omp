YCMD:istovari(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if(PI[playerid][pPosao] != POSAO_DOSTAVLJAC) return SCM(playerid,-1,""SERVERPLAVA"(MG:RPG) "C_BELA"Niste dostavljac.");
	if(!DostavljacVozila(GetPlayerVehicleID(playerid))) return SCM(playerid,-1,""SERVERPLAVA"(MG:RPG) "C_BELA"Niste u vozilu dostavljaca.");
	if(ProduktiVozila[GetPlayerVehicleID(playerid)] == 0) return SCM(playerid,-1,""SERVERPLAVA"(MG:RPG) "C_BELA"U vozilu nema produkata.");
	new kolcina;
	if(sscanf(params, "i", kolcina)) return Koristi(playerid, "istovari [broj produkata]");
	new usad = kolcina*2;
	for(new i = 0; i < MAX_FIRMI; i++)
	{
	    if(IsPlayerInRangeOfPoint(playerid, 15.0, FirmaInfo[i][fUlazX], FirmaInfo[i][fUlazY], FirmaInfo[i][fUlazZ]))
	    {
	 		//if(FirmaInfo[i][fVrsta] == VRSTA_SBIZZ) return SCM(playerid,-1,""C_BELA"Ne mozete dostavljati proizvode u tu firmu.");
			if(FirmaInfo[i][fImaVlasnika] != 1) return SCM(playerid,-1,""C_BELA"Ta firma nema vlasnika."); //kad bude igraa
			if(FirmaInfo[i][fProizvodi] >= 300) return SCM(playerid,-1,""C_BELA"Toj firmi nisu potrebni proizvodi.");
			if(FirmaInfo[i][fKasa] < usad) return SCM(playerid,-1,""C_BELA"Ta firma nema dovoljno novca za proizvode.");
			if(ProduktiVozila[GetPlayerVehicleID(playerid)] < kolcina) return Error(playerid, "Nemate toliko produkata u vozilu");
			ProduktiVozila[GetPlayerVehicleID(playerid)] -= kolcina;
			FirmaInfo[i][fProizvodi] += kolcina;
			FirmaInfo[i][fKasa] -= kolcina*2;
			//new bonus = randomEx(10,20);
			g_NovacPlus(playerid, usad);
			FirmaNovac(102, usad/2);
			SCMF(playerid,ZUTA,"Dostavili ste proizvode u firmu i zaradili ste %d$", usad);
			if(ProduktiVozila[GetPlayerVehicleID(playerid)] == 0)
			UzeoKutiju[playerid] = 0;
			
		}
	}
    return 1;
}
