YCMD:popravi(playerid, params[],help)
{
    if(UlogovanProvera[playerid] == 0) return Error(playerid, "Moras biti prijavljen.");
	if(PI[playerid][pPosao] != POSAO_MEHANICAR) return SCM(playerid,-1,""SERVERPLAVA"(MG:RPG) "C_BELA"Niste mehanicar.");
    if(PosaoOprema[playerid] == 0) return SCM(playerid,-1,""SERVERPLAVA"(MG:RPG) "C_BELA"Niste uzeli opremu za posao (/oprema).");
	if(IsPlayerInAnyVehicle(playerid)) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Ne mozete to dok ste u vozilu.");
 	new id, cena, Float:Poz[3];
 	if(sscanf(params, "ui", id, cena)) return Koristi(playerid, "popravi [ID/Dio Imena] [Cena popravke]");
 	if(!IsPlayerConnected(id)) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac je offline.");
 	GetPlayerPos(id, Poz[0], Poz[1], Poz[2]);
 	if(!IsPlayerInRangeOfPoint(playerid, 5.0, Poz[0], Poz[1], Poz[2])) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac nije blizu tebe.");
 	if(!IsPlayerInAnyVehicle(id)) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac mora biti u vozilu.");
 	if(GetPlayerState(id) != PLAYER_STATE_DRIVER) SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Igrac mora biti vozac.");
 	if(cena > 3000) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Cena popravke ne moze biti veca od 3000$.");
 	PonudioPopravku[id] = playerid; CenaPopravke[id] = cena;
	SCMF(playerid, SVETLOPLAVA, "* Ponudili ste popravku vozila igracu %s za %d$.", Ime_Igraca(id), cena);
	SCMF(id, SVETLOPLAVA, "* Mehanicar %s vam je ponudio popravku vozila za %d$,da prihvatite /prihvati popravku!", Ime_Igraca(playerid), cena);
	return 1;
}
