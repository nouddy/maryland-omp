YCMD:izbrisiakaunt(playerid,params[],help)
{
  	if(PI[playerid][Admin] >= 6 || IsPlayerAdmin(playerid))
	{
		new account[24];
		if(sscanf(params, "s[32]", account)) return Koristi(playerid, "izbrisiakaunt [Ime_Prezime]");
