		if(GetPlayerAmmo(playerid) < kolicina) return Error(playerid, "Nemate toliko metaka!");
		new oruzje = GetPlayerWeapon(playerid);
		new municijaplayerid = GetPlayerAmmo(playerid);
		new municijanova = municijaplayerid - kolicina;
		GivePlayerWeaponEx(id, oruzje, kolicina);
		SetPlayerAmmoEx(playerid, oruzje, municijanova);
		format(string,sizeof(string),"* %s vam je dao %d metaka %s!", Ime_Igraca(playerid), kolicina, ImeOruzija(oruzje));
	    SCM(id, ZUTA, string);
	    format(string,sizeof(string),"* Dali ste %s %s %d metaka!", Ime_Igraca(id), ImeOruzija(oruzje), kolicina);
	    SCM(playerid, ZUTA, string);
		format(nellys, sizeof(nellys), "* %s je dao metke %s. (%s)", Ime_Igraca(playerid), Ime_Igraca(id), ImeOruzija(oruzje));
		ProxDetector(30.0, playerid, nellys, PROX,PROX,PROX,PROX,PROX);
	}
	else if(!strcmp(stvar, "cannabis", true))
	{
	    if(PI[playerid][Cannabis] < kolicina) return Error(playerid, "Nemate toliko Cannabis!");
	    if(kolicina < 1 || kolicina > 2000) return Error(playerid, "Ne mozete ici ispod 1 i iznad 2000 odjednom!");
	    PI[playerid][Cannabis] -= kolicina; PI[id][Cannabis] += kolicina;
	    format(string,sizeof(string),"* Dali ste %dg Cannabis igracu %s!", kolicina, Ime_Igraca(id));
	    SCM(playerid, ZUTA, string);
		format(string,sizeof(string),"* Igrac %s vam je dao %dg Cannabis!", Ime_Igraca(playerid), kolicina);
	    SCM(id, ZUTA, string);
	    format(nellys, sizeof(nellys), "* %s vadi kesicu Cannabis i daje %s.", Ime_Igraca(playerid), Ime_Igraca(id));
		ProxDetector(30.0, playerid, nellys, PROX,PROX,PROX,PROX,PROX);
	}
	else if(!strcmp(stvar, "kokain", true))
	{
	    if(PI[playerid][Kokain] < kolicina) return Error(playerid, "Nemate toliko kokaina!");
	    if(kolicina < 1 || kolicina > 2000) return Error(playerid, "Ne mozete ici ispod 1 i iznad 2000 odjednom!");
	    PI[playerid][Kokain] -= kolicina; PI[id][Kokain] += kolicina;
	    format(string,sizeof(string),"* Dali ste %dg kokain igracu %s!", kolicina, Ime_Igraca(id));
	    SCM(playerid, ZUTA, string);
		format(string,sizeof(string),"* Igrac %s vam je dao %dg kokain!", Ime_Igraca(playerid), kolicina);
	    SCM(id, ZUTA, string);
        format(nellys, sizeof(nellys), "* %s vadi kesicu kokain i daje %s.", Ime_Igraca(playerid), Ime_Igraca(id));
		ProxDetector(30.0, playerid, nellys, PROX,PROX,PROX,PROX,PROX);
	}
	else if(!strcmp(stvar, "mats", true))
	{
	    if(PI[id][SatiIgre] < 2) return Error(playerid, "Taj igrac nema 2+ sati igranja!");
	    if(PI[playerid][pMats] < kolicina) return SCM(playerid,SERVERBOJA,"(MG:RPG) "C_BELA"Nemate toliko materijala.");
	    if(kolicina < 1 || kolicina > 2000) return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Ne mozete ici ispod 1 i iznad 2000 odjednom.");
	    PI[playerid][pMats] -= kolicina; PI[id][pMats] += kolicina;
	    format(string,sizeof(string),"** Dali ste %d materijala igracu %s!", kolicina, Ime_Igraca(id));
	    SCM(playerid, ZUTA, string);
		format(string,sizeof(string),"** Igrac %s vam je dao %d materijala!", Ime_Igraca(playerid), kolicina);
	    SCM(id, ZUTA, string);
	}
	else return SCM(playerid, BELA, ""SERVERPLAVA"(MG:RPG) "C_BELA"Pogresno ime stvari.");
	return 1;
}
YCMD:pay(playerid, params[],help)
{
	new giveplayerid, playermoney, moneys;
	if(sscanf(params, "ud", giveplayerid, moneys)) return Koristi(playerid, "pay [ID Igraca] [kolicina]");
	if(PI[playerid][pNivo] < 3) return Error(playerid,"Morate biti nivo 3+ da bi dali nekom novac");
	if(PI[playerid][SatiIgre] < 2) return Error(playerid,"Morate imati minimum jedan sat igre za koriscenje ove komande.");
	//if(moneys > 1 && PI[playerid][pNivo] < 2) return Error(playerid,"Morate biti nivo 2 da bi poslali preko 1$.");
	//if(moneys > 100 && PI[playerid][pNivo] < 3) return Error(playerid,"Morate biti nivo 3 da bi poslali preko 100.");
	if(moneys < 1 || moneys > 1000) return Error(playerid, "Ne mozete slati manje od 1, ili vise od 100000 odjednom.");
	if(giveplayerid == playerid) return Error(playerid, "Ne mozes komandu koristiti na sebi.");
	if(IsPlayerConnected(giveplayerid))
	{
 		if(giveplayerid != INVALID_PLAYER_ID)
		 {
			if(ProxDetectorS(5.0, playerid, giveplayerid))
			{
				playermoney = PI[playerid][pMoney];
				if(moneys > 0 && playermoney >= moneys)
				{
					g_NovacMinus(playerid, moneys);
					g_NovacPlus(giveplayerid, moneys);
					format(nellys, sizeof(nellys), " Dali ste %s(ID: %d), %d$.", Ime_Igraca(giveplayerid),giveplayerid, moneys);
					PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
					SCM(playerid, COLOR_GRAD1, nellys);
					format(nellys, sizeof(nellys), "Primili ste %d$ od %s(ID: %d).", moneys, Ime_Igraca(playerid), playerid);
					SendClientMessage(giveplayerid, COLOR_GRAD1, nellys);
					PlayerPlaySound(giveplayerid, 1052, 0.0, 0.0, 0.0);
					format(nellys, sizeof(nellys), "* %s vadi svoj novcanik i daje nesto novca %s.", Ime_Igraca(playerid) ,Ime_Igraca(giveplayerid));
					ProxDetector(30.0, playerid, nellys, PROX,PROX,PROX,PROX,PROX);
					ApplyAnimation(playerid,"DEALER","shop_pay",4.1,0,0,0,0,0);
					format(logstring, sizeof logstring, "| Igrac: %s | Igracu: %s | Suma novca: %d$", Ime_Igraca(playerid),Ime_Igraca(giveplayerid), moneys);
					UpisiLog(FILE_PAY, logstring);
				}
				else Error(playerid, "Nepravilan iznos transakcije.");
			}
			else Error(playerid, "Taj igrac nije dovoljno blizu vas.");
		}
	}
	else Error(playerid, "Taj igrac nije na serveru.");
	return 1;
}
