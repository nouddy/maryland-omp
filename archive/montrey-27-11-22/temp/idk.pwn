YCMD:izbaciclana(playerid, params[],help)
{
	if(PI[playerid][Admin] >= 4 || IsPlayerAdmin(playerid))
	{
	    new id;
	    if(sscanf(params, "u", id)) return Koristi(playerid, "izbaciclana [Korisnicki ID ili Ime igraca]");
		if(!IsPlayerConnected(id)) return Error(playerid, "Taj igrac je offline.");
		if(PI[playerid][Admin] < 6 && PI[id][Admin] >= 6) return Error(playerid, "Ne mozete koristiti ovu komandu na vlasniku.");
		if(PI[id][Clan] == 0 && PI[id][Lider] == 0) return Error(playerid, "Taj igrac nije clan nijedne organizacije/bande/mafije.");
		if(PI[id][Lider] > 0) return SCM(playerid, GRAD2, "Za lidere koristite /smenilidera.");
		if(PI[id][pMestoUOrg] < 1 || PI[id][pMestoUOrg] > 20) return Error(playerid, "Nepoznato mesto.");
		new m = PI[id][pMestoUOrg];
		if(PI[id][Clan] == 1) strmid(PDInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
		else if(PI[id][Clan] == 2) strmid(FBIInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
		else if(PI[id][Clan] == 3) strmid(GSFInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
		else if(PI[id][Clan] == 4) strmid(BallasInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
		else if(PI[id][Clan] == 5) strmid(SurenosInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
		else if(PI[id][Clan] == 6) strmid(VagosInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
		else if(PI[id][Clan] == 7) strmid(LCNInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
		else if(PI[id][Clan] == 8) strmid(YakuzaInfo[m][Mesto], "Niko", 0, strlen("Niko"), 255);
		else return Error(playerid, "Nepoznata organizacija.");
		PI[id][pMestoUOrg] = 0;
		PI[id][Clan] = 0;
		PI[id][Lider] = 0;
		PI[id][Rank] = 1;
		PI[id][pTimUgovor] = 0;
		PI[id][pKaznaUgovor] = 0;
		if(PI[id][Pol] == 1) { PI[id][pSkin] = 299; Update_Skin(id, 299); }
	 	else if(PI[id][Pol] == 2) { PI[id][pSkin] = 211; Update_Skin(id, 211); }
		SCMF(id, CRVENA, "Izbaceni ste iz svoje organizacije od admina %s.", Ime_Igraca(playerid));
		format(nellys, 128, "![WARNING]: Admin %s je izbacio %s iz njegove organizacije.", Ime_Igraca(playerid), Ime_Igraca(id));
		AdminPoruka(COLOR_YELLOW, nellys);

	}
	else return Ovlascenje(playerid);
	return 1;
}
